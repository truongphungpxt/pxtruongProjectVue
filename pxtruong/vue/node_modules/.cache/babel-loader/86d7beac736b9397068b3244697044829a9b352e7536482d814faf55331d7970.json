{"ast":null,"code":"import format from '@/js/format/format';\nexport default {\n  props: {\n    modelValue: null,\n    inputContent: null,\n    isNotAllowNull: Boolean,\n    isInputNumber: {\n      type: Boolean,\n      default: false\n    },\n    isDisable: {\n      type: Boolean,\n      default: false\n    },\n    isFocus: {\n      type: Boolean,\n      default: false\n    },\n    isDisableAndPaddingLeft: {\n      type: Boolean,\n      default: false\n    },\n    isPaddingLeft: {\n      type: Boolean,\n      default: false\n    },\n    isNomalInput: {\n      type: Boolean,\n      default: false\n    }\n  },\n  created() {\n    this.value = this.modelValue;\n    console.log(this.value);\n  },\n  watch: {\n    value: function (newValue) {\n      this.$emit(\"update:modelValue\", newValue);\n    }\n  },\n  mounted() {\n    //xét focus vào input mình muốn \n    if (this.isFocus) {\n      this.$nextTick(function () {\n        this.$refs[\"Minput\"].focus();\n      });\n    }\n  },\n  methods: {\n    /**\r\n     * Author:PXTRUONG, Create: 29/5/2023\r\n     * validate dữ liệu \r\n     */\n    validate(data) {\n      if (this.isNotAllowNull) {\n        if (data == null || data == undefined || data == \"\") {\n          this.isNull = true;\n          this.alertMsg = this.inputContent + \" không được bỏ trống\";\n          return true;\n        } else {\n          this.isNull = false;\n          this.format(data);\n          return false;\n        }\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(30/5/2023)\r\n     * Kiểm tra xem có value có phải dạng sô không\r\n     */\n    checkNumber(data) {\n      return Number.isFinite(data);\n    },\n    /**\r\n     * Author:PXTRUONG(30/5/2023)\r\n     * thực hiện tăng thêm 1 đơn vị khi input dạng số \r\n     */\n    btnUp() {\n      if (this.isInputNumber) {\n        //lần đầu tiên chưa nhập số gì khi nhẫn nút tăng \n        if (this.validate(this.value)) {\n          this.value = 0;\n        }\n        this.value = Number(this.value);\n        //kiểm tra số và tăng lên 1 đơn vị\n        if (this.checkNumber(this.value) == true) {\n          this.value += 1;\n          this.validate(this.value);\n        }\n        //sai định dạng thì thông báo \n        else {\n          this.isNull = true;\n          this.alertMsg = \" Nhập đúng định dạng số\";\n          return;\n        }\n      }\n    },\n    format(data) {\n      this.value = format.fomatmoney(data);\n      console.log(this.value);\n    },\n    /**\r\n    * Author:PXTRUONG(30/5/2023)\r\n    * thực hiện giảm  thêm 1 đơn vị khi input dạng số \r\n    */\n    btnDown() {\n      if (this.isInputNumber) {\n        if (this.value == null || this.value == undefined || this.value == \"\") {\n          this.value = 0;\n        }\n        this.value = Number(this.value);\n        if (this.checkNumber(this.value) == true && this.value > 0) {\n          this.validate(this.value);\n          this.value -= 1;\n        } else if (this.value <= 0) {\n          return;\n        } else {\n          this.value = \"\";\n          this.isNull = true;\n          this.alertMsg = \" Nhập đúng định dạng số\";\n          return;\n        }\n      }\n    }\n  },\n  data() {\n    return {\n      value: null,\n      isRequid: Boolean,\n      isNull: false,\n      alertMsg: null\n    };\n  }\n};","map":{"version":3,"names":["format","props","modelValue","inputContent","isNotAllowNull","Boolean","isInputNumber","type","default","isDisable","isFocus","isDisableAndPaddingLeft","isPaddingLeft","isNomalInput","created","value","console","log","watch","newValue","$emit","mounted","$nextTick","$refs","focus","methods","validate","data","undefined","isNull","alertMsg","checkNumber","Number","isFinite","btnUp","fomatmoney","btnDown","isRequid"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\base\\Minput.vue"],"sourcesContent":["<template>\r\n    <div class=\"input__container\">\r\n        <p class=\"popup__font__label--name\">\r\n            {{ inputContent }} <span v-if=\"isNotAllowNull\" class=\"popup__color--red\">*</span>\r\n        </p>\r\n        <input v-if=\"isNomalInput == true\" class=\"input__field  \" v-model=\"this.value\" @blur=\"validate(this.value)\"\r\n            :class=\"{ 'input__boder--red': isNull }\" @keydown.up=\"btnUp\" @keydown.down=\"btnDown\" ref=\"Minput\">\r\n\r\n        <input v-if=\"isDisable == true\" class=\"input__field bgr__color--gray \" disabled v-model=\"this.value\"\r\n            @blur=\"validate(this.value)\" :class=\"{ 'input__boder--red': isNull }\" @keydown.up=\"btnUp\"\r\n            @keydown.down=\"btnDown\">\r\n\r\n        <input v-if=\"isDisableAndPaddingLeft == true\" disabled type=\"number\" class=\"input__field bgr__color--gray set_text--left \"\r\n            v-model=\"this.value\" @blur=\"validate(this.value)\" :class=\"{ 'input__boder--red': isNull }\" @keydown.up=\"btnUp\"\r\n            @keydown.down=\"btnDown\">\r\n\r\n        <input v-if=\"isPaddingLeft == true\"  class=\"input__field set_text--left \" v-model=\"this.value\"\r\n            @blur=\"validate(this.value)\" :class=\"{ 'input__boder--red': isNull }\" @keydown.up=\"btnUp\"\r\n            @keydown.down=\"btnDown\" >\r\n\r\n        <div class=\"input__range\" v-if=\"isPaddingLeft \">\r\n            <div class=\" icon input__icon--dropdownup\" @click=\"btnUp\">\r\n\r\n            </div>\r\n            <div class=\"icon input__icon--dropdown\" @click=\"btnDown\">\r\n\r\n            </div>\r\n        </div>\r\n        <div class=\"popup__color--red\" v-if=\"isNull\">\r\n            {{ alertMsg }}\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport format from '@/js/format/format';\r\nexport default {\r\n    props: {\r\n        modelValue: null,\r\n        inputContent: null,\r\n        isNotAllowNull: Boolean,\r\n        isInputNumber: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        isDisable: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        isFocus: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        isDisableAndPaddingLeft: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        isPaddingLeft: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        isNomalInput: {\r\n            type: Boolean,\r\n            default: false\r\n        }\r\n    },\r\n    created() {\r\n        this.value = this.modelValue;\r\n        console.log(this.value);\r\n    },\r\n    watch: {\r\n        value: function (newValue) {\r\n            this.$emit(\"update:modelValue\", newValue);\r\n        }\r\n    },\r\n    mounted() {\r\n        //xét focus vào input mình muốn \r\n        if (this.isFocus) {\r\n            this.$nextTick(function () {\r\n                this.$refs[\"Minput\"].focus();\r\n            })\r\n        }\r\n\r\n\r\n    },\r\n\r\n    methods: {\r\n        /**\r\n         * Author:PXTRUONG, Create: 29/5/2023\r\n         * validate dữ liệu \r\n         */\r\n        validate(data) {\r\n            if (this.isNotAllowNull) {\r\n                if (data == null || data == undefined || data == \"\") {\r\n                    this.isNull = true;\r\n                    this.alertMsg = this.inputContent + \" không được bỏ trống\"\r\n                    return true;\r\n                }\r\n                else {\r\n                    this.isNull = false;\r\n                    this.format(data);\r\n                    return false;\r\n                }\r\n            }\r\n\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(30/5/2023)\r\n         * Kiểm tra xem có value có phải dạng sô không\r\n         */\r\n        checkNumber(data) {\r\n            return Number.isFinite(data);\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(30/5/2023)\r\n         * thực hiện tăng thêm 1 đơn vị khi input dạng số \r\n         */\r\n        btnUp() {\r\n            if (this.isInputNumber) {\r\n                //lần đầu tiên chưa nhập số gì khi nhẫn nút tăng \r\n                if (this.validate(this.value)) {\r\n                    this.value = 0;\r\n                }\r\n                this.value = Number(this.value);\r\n                //kiểm tra số và tăng lên 1 đơn vị\r\n                if (this.checkNumber(this.value) == true) {\r\n\r\n                    this.value += 1;\r\n                    this.validate(this.value);\r\n\r\n                }\r\n                //sai định dạng thì thông báo \r\n                else {\r\n                    this.isNull = true\r\n                    this.alertMsg = \" Nhập đúng định dạng số\";\r\n\r\n                    return\r\n\r\n\r\n                }\r\n\r\n            }\r\n\r\n\r\n        },\r\n        format(data){\r\n            this.value= format.fomatmoney(data);\r\n            console.log(this.value);\r\n        },\r\n        /**\r\n        * Author:PXTRUONG(30/5/2023)\r\n        * thực hiện giảm  thêm 1 đơn vị khi input dạng số \r\n        */\r\n        btnDown() {\r\n            if (this.isInputNumber) {\r\n                if (this.value == null || this.value == undefined || this.value == \"\") {\r\n                    this.value = 0;\r\n                }\r\n                this.value = Number(this.value);\r\n                if (this.checkNumber(this.value) == true && this.value > 0) {\r\n\r\n                    this.validate(this.value);\r\n                    this.value -= 1;\r\n                }\r\n                else if (this.value <= 0) {\r\n\r\n                    return\r\n                }\r\n                else {\r\n                    this.value = \"\";\r\n                    this.isNull = true\r\n                    this.alertMsg = \" Nhập đúng định dạng số\";\r\n                    return\r\n                }\r\n            }\r\n\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            value: null,\r\n            isRequid: Boolean,\r\n            isNull: false,\r\n            alertMsg: null\r\n\r\n        }\r\n    },\r\n}\r\n</script>\r\n<style scoped>\r\n@import url(../../style/base/textfield.css);\r\n@import url(../../style/base/icon.css);\r\n</style>"],"mappings":"AAkCA,OAAOA,MAAK,MAAO,oBAAoB;AACvC,eAAe;EACXC,KAAK,EAAE;IACHC,UAAU,EAAE,IAAI;IAChBC,YAAY,EAAE,IAAI;IAClBC,cAAc,EAAEC,OAAO;IACvBC,aAAa,EAAE;MACXC,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDC,SAAS,EAAE;MACPF,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDE,OAAO,EAAE;MACLH,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDG,uBAAuB,EAAE;MACrBJ,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDI,aAAa,EAAE;MACXL,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDK,YAAY,EAAE;MACVN,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb;EACJ,CAAC;EACDM,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,KAAI,GAAI,IAAI,CAACb,UAAU;IAC5Bc,OAAO,CAACC,GAAG,CAAC,IAAI,CAACF,KAAK,CAAC;EAC3B,CAAC;EACDG,KAAK,EAAE;IACHH,KAAK,EAAE,SAAAA,CAAUI,QAAQ,EAAE;MACvB,IAAI,CAACC,KAAK,CAAC,mBAAmB,EAAED,QAAQ,CAAC;IAC7C;EACJ,CAAC;EACDE,OAAOA,CAAA,EAAG;IACN;IACA,IAAI,IAAI,CAACX,OAAO,EAAE;MACd,IAAI,CAACY,SAAS,CAAC,YAAY;QACvB,IAAI,CAACC,KAAK,CAAC,QAAQ,CAAC,CAACC,KAAK,CAAC,CAAC;MAChC,CAAC;IACL;EAGJ,CAAC;EAEDC,OAAO,EAAE;IACL;;;;IAIAC,QAAQA,CAACC,IAAI,EAAE;MACX,IAAI,IAAI,CAACvB,cAAc,EAAE;QACrB,IAAIuB,IAAG,IAAK,IAAG,IAAKA,IAAG,IAAKC,SAAQ,IAAKD,IAAG,IAAK,EAAE,EAAE;UACjD,IAAI,CAACE,MAAK,GAAI,IAAI;UAClB,IAAI,CAACC,QAAO,GAAI,IAAI,CAAC3B,YAAW,GAAI,sBAAqB;UACzD,OAAO,IAAI;QACf,OACK;UACD,IAAI,CAAC0B,MAAK,GAAI,KAAK;UACnB,IAAI,CAAC7B,MAAM,CAAC2B,IAAI,CAAC;UACjB,OAAO,KAAK;QAChB;MACJ;IAGJ,CAAC;IACD;;;;IAIAI,WAAWA,CAACJ,IAAI,EAAE;MACd,OAAOK,MAAM,CAACC,QAAQ,CAACN,IAAI,CAAC;IAChC,CAAC;IACD;;;;IAIAO,KAAKA,CAAA,EAAG;MACJ,IAAI,IAAI,CAAC5B,aAAa,EAAE;QACpB;QACA,IAAI,IAAI,CAACoB,QAAQ,CAAC,IAAI,CAACX,KAAK,CAAC,EAAE;UAC3B,IAAI,CAACA,KAAI,GAAI,CAAC;QAClB;QACA,IAAI,CAACA,KAAI,GAAIiB,MAAM,CAAC,IAAI,CAACjB,KAAK,CAAC;QAC/B;QACA,IAAI,IAAI,CAACgB,WAAW,CAAC,IAAI,CAAChB,KAAK,KAAK,IAAI,EAAE;UAEtC,IAAI,CAACA,KAAI,IAAK,CAAC;UACf,IAAI,