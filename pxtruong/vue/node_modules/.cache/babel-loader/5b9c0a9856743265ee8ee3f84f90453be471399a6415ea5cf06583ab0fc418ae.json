nsole.log(option);\n      await fetch(pelfApi.fixedAsset.api, option).then(res => res.json()).then(data => {\n        console.log(data);\n      });\n    },\n    /**\r\n    * Author:PXTRUONG(15/6/2023)\r\n    * Thực hiện gọi api sửa tài sản \r\n    */\n    async fix() {\n      console.log(JSON.stringify(this.pelfInput));\n      let optionFix = {\n        method: 'PUT',\n        headers: {\n          'Content-Type': 'application/json;charset=utf-8'\n        },\n        body: JSON.stringify(this.pelfInput)\n      };\n      console.log(JSON.stringify(this.pelfInput));\n      await fetch(pelfApi.fixedAsset.api, optionFix).then(res => res.json()).then(data => {\n        console.log(data);\n      });\n    },\n    /**\r\n     * Author:PXTRUONG(31/5/2023)\r\n     * Thực hiện lưu và đóng form \r\n     */\n    async btnSave() {\n      // this.btnClose();\n      try {\n        let listInValid = [];\n        this.isCheckValidate = true;\n        this.$eventBus.on(\"listInValid\", data => {\n          listInValid = data;\n        });\n        console.log(listInValid);\n\n        // if (this.formMode == this.$_MFormMode.FixAsset) {\n        //     this.fix();\n        // }\n        // else if (this.formMode == this.$_MFormMode.AddAsset) {\n        //     this.add();\n        // }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(31/5/2023)\r\n     * Đống form và không lưu lại dữ liệu \r\n     */\n    btnCloseUnSave() {\n      this.$emit(\"update:pelf\", this.pelftDefault);\n    },\n    /**\r\n     * \r\n     * Author:PXTRUONG(31/5/2023)\r\n     * thay đổi dữ liệu phụ thuộc khi thay đổi mã tài sản,loại tài sản \r\n     */\n    async bindData(api, value, protext) {\n      let option = {\n        method: 'GET',\n        headers: {\n          'Content-Type': 'application/json;charset=utf-8'\n        }\n      };\n      await fetch(api + \"?recodeCode=\" + value, option).then(res => res.json()).then(data => {\n        data.forEach(