ey(item.Cost-1200000) }}\r\n                    </td>\r\n                    <td>\r\n                        <div class=\" table__body--display\">\r\n                            <div class=\"icon body__table__icon--pencal\" @click=\"showPeltDetail(item)\" title=\"Chỉnh sửa\"></div>\r\n                            <div class=\"icon body__table__icon--duplicate\"  title=\"Nhân bản\"></div>\r\n                        </div>\r\n                    </td>\r\n\r\n                </tr>\r\n          \r\n                <div>\r\n\r\n                </div>\r\n\r\n            </tbody>\r\n    \r\n\r\n\r\n\r\n            <tfoot class=\"table__footer \">\r\n                <tr>\r\n                    <td colspan=\"6\">\r\n                       \r\n\r\n\r\n                    </td>\r\n                    <td class=\"text--right\">\r\n                        13\r\n                    </td>\r\n                    <td class=\"text--right\">\r\n                        249.000.000\r\n                    </td>\r\n                    <td class=\"text--right\">\r\n                        19.716.000\r\n                    </td>\r\n                    <td class=\"text--right\">\r\n                        229.248.000\r\n                    </td>\r\n                    <td>\r\n\r\n                    </td>\r\n                </tr>\r\n            </tfoot>\r\n            <div class=\"table__footer--container\">\r\n                            <div class=\"table__footer--left\">\r\n                                <div>\r\n                                    <p class=\"table__footer__font--regular\">\r\n                                        Tổng số : <span class=\"table__footer__font--bold\"> 200</span> bản ghi\r\n                                    </p>\r\n                                </div>\r\n\r\n                                <div class=\"table__footer--containerbutton\">\r\n                                    <input class=\"input__field unset__margin tfoot__custom--setwidth\" placeholder