cked = listDelete\n        this.listChecked = this.listChecked.filter(item => !this.findFixedAssetCode().includes(item));\n        console.log(\"uncheck\", this.listChecked);\n      } else {\n        this.listChecked = this.findFixedAssetCode();\n        console.log(this.listChecked);\n      }\n\n      // return listChecked;\n    },\n\n    /**\r\n     * Author:PXTRUONG(21/6/2023)\r\n     * Chuyền dữ liệu sau khi phan trang \r\n     */\n    bindDate(data) {\n      this.listData = data;\n    },\n    /**\r\n    * Load dữ liệu từ api cho vào bảng\r\n    * Author:Phùng Xuân Trường\r\n    * 24/2/2023\r\n    */\n    async loadData() {\n      await fetch(pelfApi.fixedAsset.getPaging + '?PageSize= 10' + '&PageNumber= 1').then(res => res.json()).then(value => {\n        this.listData = value.data;\n        this.totalRecord = value.data[0].TotalRecord;\n        ;\n      });\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Kiểm tra mã có trong list được tích hay không\r\n     */\n    chekedList(data, index) {\n      if (this.listChecked.includes(data)) {\n        return true;\n      } else if (index == this.indexSelected) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Đổi màu khi click chọn\r\n     */\n    selectItem(index, data) {\n      if (this.listChecked.includes(data)) {\n        this.listChecked = this.listChecked.filter(item => item !== data);\n        this.indexSelected = -1;\n      } else {\n        this.indexSelected = index;\n        this.listChecked.push(data);\n      }\n      console.log(this.listChecked);\n      this.$eventBus.emit(\"listCodeDelete\", this.listChecked);\n    },\n    /**\r\n     * Author:PXTRUONG, Create: 27/5/2023\r\n     * gọi hàm format tiền\r\n     */\n    formatMoney(money) {\n      return format.fomatmoney(money);\n    },\n    /**\r\n     * Author:PXTRUONG, Create: 27/5/2023\r\n     */\n    showPeltDetail(data) {\n      this.$emit(\"showDetail\", data, this.$_MFormMode.FixAsset);\n    }\n  },\n  data() {\n    return {\n      data: [],\n      tableLabel: resource.tableTitle,\n      listChecked: [],\n      listData: [],\n      indexSelected: null,\n      renderComponent: true,\n      isNull: false,\n      listPage: [{\n        \"PageNumber\": \"20\"\n      }, {\n        \"PageNumber\": \"30\"\n      }, {\n        \"PageNumber\": \"40\"\n      }, {\n        \"PageNumber\": \"50\"\n      }],\n      setWidth: [],\n      totalRecord: null,\n      keyWordInput: null,\n      startXMouse: null,\n      ischeckall: false\n    };\n  }\n};","map":{"version":3,"names":["pelfApi","format","resource","MCheckBox","MDropdown","MPaging","components","created","loadData","watch","listChecked","newValue","$eventBus","emit","isDeleteSucess","forEach","element","listData","elementData","FixedAssetCode","console","log","splice","data","indexOf","$emit","length","isNull","props","Boolean","methods","findFixedAssetCode","push","checkAll","ischeckall","filter","item","includes","bindDate","fetch","fixedAsset","getPaging","then","res","json","value","totalRecord","TotalRecord","chekedList","index","indexSelected","selectItem","formatMoney","money","fomatmoney","showPeltDetail","$_MFormMode","FixAsset","tableLabel","tableTitle","renderComponent","listPage","setWidth","keyWordInput","startXMouse"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\view\\pelf\\ListPelf.vue"],"sourcesContent":["<template>\r\n    <div class=\"body__content\" v-if=\"this.renderComponent == true\">\r\n\r\n        <table class=\"table__content\">\r\n            <thead class=\"table__header\">\r\n                <th class=\"  th__checkbox table__Maxwidth\">\r\n                    <input type=\"checkbox\" class=\"input__checkbox input__checkbox--icon\" v-model=\"ischeckall\"\r\n                        @click=\"checkAll\">\r\n                </th>\r\n                <th title=\"Số thứ tự\" class=