<td @click=\"selectItem(index, item.FixedAssetCode)\" style=\"min-width: 179px;\">\r\n                        {{ item.FixedAssetName }}\r\n                    </td>\r\n                    <td @click=\"selectItem(index, item.FixedAssetCode)\" style=\"min-width: 149px;\">\r\n                        {{ item.FixedAssetCategoryName }}\r\n                    </td>\r\n                    <td @click=\"selectItem(index, item.FixedAssetCode)\" style=\"min-width: 145px;\">\r\n                        {{ item.DepartmentName }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\" style=\"min-width: 89px;\">\r\n                        {{ item.Quantity }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\" style=\"min-width: 121px;\">\r\n                        {{ this.formatMoney(item.Cost)\r\n\r\n                        }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\" style=\"min-width: 123px;\">\r\n                        {{ this.formatMoney(1200000) }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\" style=\"min-width: 121px;\">\r\n                        {{ this.formatMoney(item.Cost - 1200000) }}\r\n                    </td>\r\n                    <td>\r\n                        <div class=\" table__body--display\">\r\n                            <div class=\"icon body__table__icon--pencal\" @click=\"showPeltDetail(item)\" title=\"Chỉnh sửa\">\r\n                            </div>\r\n                            <div class=\"icon body__table__icon--duplicate\" title=\"Nhân bản\"></div>\r\n                        </div>\r\n                    </td>\r\n\r\n                </tr>\r\n\r\n\r\n\r\n                <div>\r\n\r\n                </div>\r\n\r\n            </tbody>\r