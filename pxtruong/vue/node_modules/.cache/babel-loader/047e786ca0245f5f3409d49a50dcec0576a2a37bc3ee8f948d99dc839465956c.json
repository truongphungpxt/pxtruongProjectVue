{"ast":null,"code":"import format from \"@/js/format/format\";\nimport { ElDropdown, timelineItemProps } from \"element-plus\";\nimport { ArrowDown } from \"@element-plus/icons-vue\";\nimport { get } from '@/js/services/sendrequest';\nexport default {\n  components: {\n    ElDropdown,\n    ArrowDown\n  },\n  props: {\n    isDisable: false,\n    inputContent: null,\n    iconDropdown: null,\n    isRequire: Boolean,\n    isNotAllowNull: Boolean,\n    propText: null,\n    modelValue: null,\n    api: null,\n    isCheckValidate: {\n      type: Boolean,\n      default: false\n    },\n    isShowError: {\n      type: Boolean,\n      default: true\n    },\n    isUp: {\n      type: Boolean,\n      default: false\n    },\n    listDataInput: [],\n    setMaxLength: null\n  },\n  watch: {\n    modelValue: function (newValue) {\n      this.value = newValue;\n    },\n    value: function (newValue) {\n      this.$emit(\"update:modelValue\", newValue);\n    },\n    isCheckValidate: function (newValue) {\n      if (newValue && this.isNotAllowNull) {\n        this.validate(this.value);\n      }\n    }\n  },\n  created() {\n    this.value = this.modelValue;\n    if (this.api) {\n      // fetch(this.api)\n      //   .then(res => res.json())\n      //   .then((data) => {\n      //     this.listData = data;\n      //     this.origilData = data;\n      //     this.itermsFilter = data;\n      //   })\n      //   ;\n      this.loaddata(this.api);\n    }\n    if (this.listDataInput) {\n      this.itermsFilter = this.listDataInput;\n      this.origilData = this.listDataInput;\n    }\n  },\n  data() {\n    return {\n      isshow: false,\n      nowindex: 0,\n      value: null,\n      isNull: false,\n      alertMsg: null,\n      itermsFilter: [],\n      origilData: [],\n      listData: [],\n      indexSelected: -1,\n      isNotFound: false\n    };\n  },\n  methods: {\n    /**\r\n     * Author:PXTRUONG(28/6/2023)\r\n     * Load dữ liệu \r\n     */\n    async loaddata(api) {\n      try {\n        var data = await get(api);\n        console.log(data);\n        this.listData = data;\n        this.origilData = data;\n        this.itermsFilter = data;\n      } catch (error) {\n        throw error;\n      }\n    },\n    /**\r\n       * Author:PXTRUONG(27/6/2023)\r\n       * Thực hiện focus \r\n       */\n    focus() {\n      this.$nextTick(function () {\n        this.$refs[\"Minput\"].focus();\n      });\n    },\n    /**\r\n     * Author:PXTRUONG(25/6/2023)\r\n     * gửi dữ liệu để lọc theo dữ liệu truyền vào cho list\r\n     */\n    sortData() {\n      if (this.propText == this.$_MResource.PropText.FixedAssetCategory.FixedAssetCategoryName) {\n        this.$eventBus.emit(\"FixedAssetCategoryName\", this.value);\n      } else if (this.propText == this.$_MResource.PropText.Department.DepartmentName) {\n        this.$eventBus.emit(\"DepartmentName\", this.value);\n      }\n    },\n    /**\r\n     * Author: PXTRUONG(5/6/2023)\r\n     * đóng mở danh list dropdown\r\n     */\n    showClick() {\n      if (this.isshow == false) {\n        this.isshow = true;\n      } else {\n        this.isshow = false;\n      }\n    },\n    /**\r\n     * Author:PXTURONG(5/6/2023)\r\n     * Autocomplite \r\n     */\n    filterIterms(data) {\n      try {\n        if (this.checknull(format.whiteSpace(data))) {\n          this.itermsFilter = this.origilData;\n          this.isshow = true;\n        } else {\n          this.itermsFilter = this.origilData.filter(item => item[this.propText].includes(format.whiteSpace(data)));\n          this.isshow = true;\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * Đóng form\r\n     */\n    clostab() {\n      this.isshow = false;\n    },\n    /**\r\n     * Author:PXTRUONG(5/5/2023)\r\n     * thực hiện phím tắt để chọn item \r\n     */\n    kewdown(event) {\n      try {\n        const maxLength = this.filterIterms.length;\n        if (maxLength == 0) {\n          return;\n        } else {\n          if (event.keyCode == 40) {\n            this.isshow = true;\n            if (this.indexSelected <= maxLength + 1) {\n              this.indexSelected++;\n            } else {\n              this.indexSelected = 0;\n            }\n          } else if (event.keyCode == 38) {\n            if (this.indexSelected > 0) {\n              this.indexSelected--;\n            } else {\n              this.indexSelected = maxLength + 2;\n            }\n          } else if (event.keyCode == 13) {\n            this.itermsFilter = this.origilData;\n            this.value = this.itermsFilter[this.indexSelected][this.propText];\n            this.indexSelected = -1;\n            this.$emit(\"sendData\", this.api, this.value, this.propText);\n            this.clostab();\n          }\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * nhận giá trị khi click vào item mk chọn \r\n     */\n    async onSelectItem(item) {\n      try {\n        this.value = item[this.propText];\n        this.validate(this.value);\n        this.indexSelected = -1;\n        this.$emit(\"sendData\", this.api, this.value, this.propText);\n        this.clostab();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    checknull(data) {\n      data = format.whiteSpace(data);\n      if (!data) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * validate dữ liệu \r\n     */\n    validate(data) {\n      try {\n        if (this.isNotAllowNull) {\n          if (this.checknull(data)) {\n            this.isNull = true;\n            this.alertMsg = this.inputContent + \" không được bỏ trống\";\n            // console.log( this.$refs.Minput);\n            this.clostab();\n            return true;\n          } else {\n            this.isNull = false;\n            this.clostab();\n            return false;\n          }\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["format","ElDropdown","timelineItemProps","ArrowDown","get","components","props","isDisable","inputContent","iconDropdown","isRequire","Boolean","isNotAllowNull","propText","modelValue","api","isCheckValidate","type","default","isShowError","isUp","listDataInput","setMaxLength","watch","newValue","value","$emit","validate","created","loaddata","itermsFilter","origilData","data","isshow","nowindex","isNull","alertMsg","listData","indexSelected","isNotFound","methods","console","log","error","focus","$nextTick","$refs","sortData","$_MResource","PropText","FixedAssetCategory","FixedAssetCategoryName","$eventBus","emit","Department","DepartmentName","showClick","filterIterms","checknull","whiteSpace","filter","item","includes","clostab","kewdown","event","maxLength","length","keyCode","onSelectItem"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\base\\MDropdown.vue"],"sourcesContent":["<template>\r\n  <div class=\"input__container \">\r\n\r\n    <p class=\"popup__font__label--name \" v-if=\"inputContent\">\r\n      {{ inputContent }}<span v-if=\"isNotAllowNull\" class=\"popup__color--red\">*</span>\r\n    </p>\r\n    <!-- hiện input không có  icon -->\r\n    <div class=\"mcombobox__container\" v-if=\"!iconDropdown\" @click=\"showClick\" >\r\n\r\n      <input class=\"input__field\" type=\"text\" v-model=\"this.value\" :class=\"{ 'input__boder--red': isNull }\"\r\n        @blur=\"validate(this.value)\" @input=\"filterIterms(this.value)\" @keydown=\"kewdown\" ref=\"Minput\"\r\n        :maxlength=\"setMaxLength\" :disabled=\"isDisable\" />\r\n\r\n      <div  class=\"mcombobox__icon--arrowdown  icon\" @keydown.down=\"kewdown\"></div>\r\n\r\n\r\n      <div class=\"popup__error\" v-if=\"isShowError\">\r\n        <div class=\"popup__color--red\" v-if=\"isNull\">\r\n          {{ alertMsg }}\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n    <!-- hiện input có icon -->\r\n    <div class=\"mcombobox__container  \" v-if=\"iconDropdown\">\r\n      <div class=\"icon dropdown__icon--position \" :class=\"iconDropdown\" v-if=\"iconDropdown\" @click=\"sortData\"></div>\r\n      <input class=\"input__field input__padding--30px\" type=\"text\" :class=\"{ 'input__boder--red': isNull }\"\r\n        v-model=\"this.value\" @blur=\"validate(this.value)\" @input=\"filterIterms(this.value)\" @keydown=\"kewdown\"\r\n        @click=\"showClick\" ref=\"Minput\" />\r\n\r\n      <div @click=\"showClick\" class=\"mcombobox__icon--arrowdown  icon\" @keydown.down=\"kewdown\"></div>\r\n      <div class=\"popup__error\" v-if=\"isShowError\">\r\n        <div class=\"popup__color--red\" v-if=\"isNull\">\r\n          {{ alertMsg }}\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n    <!-- hiện list khi chọn khi nhập được nội dung  -->\r\n    <div class=\"mcombobox__listd\" v-show=\"isshow\" :class=\"{ 'mcombobox__listed--up': isUp }\" v-if=\"isDisable\" v-click-outside=\" this.clostab()\">\r\n      <div class=\"mcombobox__item\" v-for=\"(item, index) in itermsFilter\" :key=\"index\" @click=\"onSelectItem(item)\"\r\n        :class=\"{ 'is--sellected': item[propText] == this.value }\">\r\n        {{\r\n          item[propText]\r\n        }}\r\n      </div>\r\n\r\n      <div v-if=\"itermsFilter.length <= 0\" class=\"mcombobox__item\">\r\n        không tìm thấy dữ liệu\r\n      </div>\r\n\r\n\r\n    </div>\r\n    <!-- hiện list khi chọn theo diable -->\r\n    <div class=\"mcombobox__listd\" v-show=\"isshow\" :class=\"{ 'mcombobox__listed--up': isUp }\" v-if=\"!isDisable\" v-click-outside=\" this.clostab()\">\r\n      <div class=\"mcombobox__item\" v-for=\"(item, index) in itermsFilter\" :key=\"index\" @click=\"onSelectItem(item)\"\r\n        :class=\"{ 'is--focus': index == indexSelected, 'is--sellected': item[propText] == this.value }\">\r\n        {{\r\n          item[propText]\r\n        }}\r\n      </div>\r\n\r\n      <div v-if=\"itermsFilter.length <= 0\" class=\"mcombobox__item\">\r\n        không tìm thấy dữ liệu\r\n      </div>\r\n\r\n\r\n    </div>\r\n\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport format from \"@/js/format/format\";\r\nimport { ElDropdown, timelineItemProps } from \"element-plus\";\r\nimport { ArrowDown } from \"@element-plus/icons-vue\";\r\nimport { get } from '@/js/services/sendrequest';\r\nexport default {\r\n  components: {\r\n    ElDropdown,\r\n    ArrowDown,\r\n  },\r\n  props: {\r\n    isDisable: false,\r\n    inputContent: null,\r\n    iconDropdown: null,\r\n    isRequire: Boolean,\r\n    isNotAllowNull: Boolean,\r\n    propText: null,\r\n    modelValue: null,\r\n    api: null,\r\n    isCheckValidate: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    isShowError: {\r\n      type: Boolean,\r\n      default: true\r\n    },\r\n    isUp: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    listDataInput: [],\r\n    setMaxLength: null\r\n\r\n  },\r\n  watch: {\r\n    modelValue: function (newValue) {\r\n      this.value = newValue;\r\n    },\r\n    value: function (newValue) {\r\n\r\n      this.$emit(\"update:modelValue\", newValue);\r\n    },\r\n    isCheckValidate: function (newValue) {\r\n      if (newValue && this.isNotAllowNull) {\r\n        this.validate(this.value)\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n\r\n    this.value = this.modelValue;\r\n    if (this.api) {\r\n      // fetch(this.api)\r\n      //   .then(res => res.json())\r\n      //   .then((data) => {\r\n      //     this.listData = data;\r\n      //     this.origilData = data;\r\n      //     this.itermsFilter = data;\r\n      //   })\r\n      //   ;\r\n      this.loaddata(this.api);\r\n    }\r\n    if (this.listDataInput) {\r\n      this.itermsFilter = this.listDataInput;\r\n      this.origilData = this.listDataInput;\r\n\r\n    }\r\n\r\n\r\n\r\n  },\r\n  data() {\r\n    return {\r\n      isshow: false,\r\n      nowindex: 0,\r\n      value: null,\r\n      isNull: false,\r\n      alertMsg: null,\r\n      itermsFilter: [],\r\n      origilData: [],\r\n      listData: [],\r\n      indexSelected: -1,\r\n      isNotFound: false\r\n\r\n    }\r\n  },\r\n  methods: {\r\n    /**\r\n     * Author:PXTRUONG(28/6/2023)\r\n     * Load dữ liệu \r\n     */\r\n    async loaddata(api) {\r\n      try {\r\n        var data = await get(api);\r\n        console.log(data);\r\n        this.listData = data;\r\n        this.origilData = data;\r\n        this.itermsFilter = data;\r\n      } catch (error) {\r\n        throw error\r\n      }\r\n\r\n    },\r\n    /**\r\n       * Author:PXTRUONG(27/6/2023)\r\n       * Thực hiện focus \r\n       */\r\n    focus() {\r\n      this.$nextTick(function () {\r\n        this.$refs[\"Minput\"].focus();\r\n      })\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(25/6/2023)\r\n     * gửi dữ liệu để lọc theo dữ liệu truyền vào cho list\r\n     */\r\n    sortData() {\r\n     \r\n      if (this.propText == this.$_MResource.PropText.FixedAssetCategory.FixedAssetCategoryName) {\r\n        \r\n        this.$eventBus.emit(\"FixedAssetCategoryName\", this.value);\r\n      }\r\n      else if (this.propText == this.$_MResource.PropText.Department.DepartmentName) {\r\n        \r\n        this.$eventBus.emit(\"DepartmentName\", this.value);\r\n      }\r\n    },\r\n    /**\r\n     * Author: PXTRUONG(5/6/2023)\r\n     * đóng mở danh list dropdown\r\n     */\r\n    showClick() {\r\n\r\n      if (this.isshow == false) {\r\n\r\n        this.isshow = true;\r\n\r\n      }\r\n      else {\r\n        this.isshow = false;\r\n      }\r\n\r\n\r\n    },\r\n    /**\r\n     * Author:PXTURONG(5/6/2023)\r\n     * Autocomplite \r\n     */\r\n    filterIterms(data) {\r\n      try {\r\n\r\n        if (this.checknull(format.whiteSpace(data))) {\r\n          this.itermsFilter = this.origilData;\r\n          this.isshow = true;\r\n        }\r\n        else {\r\n          this.iter