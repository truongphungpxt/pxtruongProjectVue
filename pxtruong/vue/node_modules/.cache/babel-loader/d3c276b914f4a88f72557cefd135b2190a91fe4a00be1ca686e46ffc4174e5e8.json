         else{\r\n                this.isNull=false;\r\n            }\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(30/5/2023)\r\n         * Kiểm tra xem có value có phải dạng sô không\r\n         */\r\n        checkNumber(data){\r\n           return Number.isFinite(data);\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(30/5/2023)\r\n         * thực hiện tăng thêm 1 đơn vị khi input dạng số \r\n         */\r\n        btnUp(){\r\n            this.value=Number(this.value);\r\n            if(this.checkNumber(this.value)==true){\r\n              \r\n                this.value +=1;\r\n            }\r\n            else{\r\n                this.value=\"\";\r\n                this.isNull=true\r\n                this.alertMsg=\" Nhập đúng định dạng số\";\r\n                console.log(\"ko phải số\");\r\n                return\r\n            }\r\n          \r\n        },\r\n         /**\r\n         * Author:PXTRUONG(30/5/2023)\r\n         * thực hiện giảm  thêm 1 đơn vị khi input dạng số \r\n         */\r\n         btnDown(){\r\n           \r\n            if(this.checkNumber(this.value)){\r\n              \r\n                this.value -=1;\r\n            }\r\n            else{\r\n                this.value=\"\";\r\n                this.isNull=true\r\n                this.alertMsg=\" Nhập đúng định dạng số\";\r\n                return\r\n            }\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            value:null,\r\n            isRequid:Boolean,\r\n            isNull:false,\r\n            alertMsg:null\r\n          \r\n        }\r\n    },\r\n}\r\n</script>\r\n<style scoped>\r\n@import url(../../style/base/textfield.css);\r\n@import url(../../style/base/icon.css);\r\n</style>"],"mappings":"AAoBA,eAAe;EACXA,KAAK,EAAC;IACFC,UAAU,EAAC,CAACC,MAAM,CAAC;IACnBC,YAAY,EAAC,IAAI;IACjBC,gBAAgB,EAACC,OAAO;IACxBC,aAAa,EAAC;MACVC,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAC;IACZ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,KAAK,GAAC,IAAI,CAACT,UAAU;IAC1