{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport format from '@/js/format/format';\nimport MAlert from '@/components/base/MAlert.vue';\nimport pelfApi from '@/config/pelf/pelfapi';\nimport Minput from \"@/components/base/Minput.vue\";\nimport MinputNumber from \"@/components/base/MInputNumber.vue\";\nimport MDropdown from \"@/components/base/MDropdown.vue\";\nimport MDatapicker from \"@/components/base/MDatapicker.vue\";\nimport MNotification from '@/components/base/MNotification.vue';\nimport resource from \"@/js/resource/resource\";\nimport maxlengthinput from \"@/js/enum/maxlengthinput.js\";\nexport default {\n  components: {\n    Minput,\n    MDropdown,\n    MDatapicker,\n    MinputNumber,\n    MAlert,\n    MNotification\n  },\n  props: {\n    pelf: null,\n    formMode: null\n  },\n  created() {\n    var now = new Date();\n    this.pelftDefault = JSON.stringify(this.pelf);\n    this.pelftDefault = JSON.parse(this.pelftDefault);\n    this.pelfInput = JSON.stringify(this.pelf);\n    this.pelfInput = JSON.parse(this.pelfInput);\n    if (this.formMode == this.$_MFormMode.FixAsset) {\n      this.popupTitle = resource.FormModeTitle.FixAsset;\n    } else if (this.formMode == this.$_MFormMode.AddAsset) {\n      this.popupTitle = resource.FormModeTitle.AddAsset;\n      this.findMaxCode();\n      this.pelfInput.TrackedYear = now.getFullYear();\n    }\n    this.atrophyYear = this.pelfInput.Cost * this.pelfInput.DepreciationRate / 100;\n  },\n  mounted() {\n    //xét focus vào input mình muốn \n\n    this.$nextTick(function () {\n      this.$refs.FixedAssetCode.$refs.Minput.focus();\n    });\n  },\n  methods: {\n    /**\r\n     * Author:PXTRUONG(24/6/2023)\r\n     * Chuyển tab lên trang đầu \r\n     */\n    focusTop(event) {\n      if (event.keyCode == 9) {\n        // this.$nextTick(function () {\n        //     for (const inputRef in this.$refs) {\n        //         //tìm các input invalid\n        //         if (inputRef==\"FixedAssetCode\") {\n        //             this.$refs[inputRef].$refs.Minput.focus()\n        //             return;\n\n        //         }\n        //     }\n        // this.$refs.FixedAssetCode.$refs.Minput.focus();\n        //     })\n        this.$refs[\"FixedAssetCode\"].$refs.Minput.focus();\n        return;\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(22/6/2023)\r\n     * Tìm tìm FixedassetCode lớn nhất \r\n     */\n    async findMaxCode() {\n      var maxCode;\n      await fetch(pelfApi.fixedAsset.maxCode).then(res => res.text()).then(data => {\n        maxCode = data;\n      });\n      this.pelfInput.FixedAssetCode = maxCode;\n      console.log(maxCode);\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Đóng mở cảnh báo \r\n     */\n    closeAlert(isForm) {\n      if (isForm) {\n        this.isShowAlert = false;\n        return;\n      }\n      if (this.inValue) {\n        this.isShowAlert = false;\n        for (const inputRef in this.$refs) {\n          //tìm các input invalid\n          if (this.checkNull(this.$refs[inputRef].value)) {\n            console.log(inputRef);\n            console.log(this.$refs[inputRef].inputContent);\n            this.$refs[inputRef].$refs.Minput.focus();\n            console.log(this.$refs[inputRef].$refs.onfocus);\n            console.log(this.$refs[inputRef].$refs);\n            return;\n          }\n        }\n      } else {\n        this.isShowAlert = false;\n        this.btnClose();\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Add list invalid vào 1 mảng \r\n     */\n    addListInValues(data, isPush) {\n      if (isPush) {\n        this.listInValue.push(data);\n      } else {\n        this.listInValue = this.listInValue.filter(item => item !== data);\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Check null\r\n     */\n    checkNull(data) {\n      data = format.whiteSpace(data);\n      if (!data) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    /**\r\n     * đóng form chi tiết tài sản \r\n     * Author: PXTRUONG, Create: 24/3/2023\r\n     */\n    btnClose() {\n      this.$emit(\"hiddePolfDetail\");\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Thực hiện gọi api thêm tài sản \r\n     */\n    async add() {\n      let option = {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json;charset=utf-8'\n        },\n        body: JSON.stringify(this.pelfInput)\n      };\n      console.log(JSON.stringify(this.pelfInput));\n      await fetch(pelfApi.fixedAsset.api, option).then(res => res.json()).then(data => {\n        if (data.IsSuccess) {\n          this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Add);\n        } else {\n          this.showInform(true, this.$_MInformStatus.IsFailed, data.UserMessage);\n        }\n      });\n    },\n    /**\r\n    * Author:PXTRUONG(15/6/2023)\r\n    * Thực hiện gọi api sửa tài sản \r\n    */\n    async fix() {\n      let optionFix = {\n        method: 'PUT',\n        headers: {\n          'Content-Type': 'application/json;charset=utf-8'\n        },\n        body: JSON.stringify(this.pelfInput)\n      };\n      console.log(JSON.stringify(this.pelfInput));\n      await fetch(pelfApi.fixedAsset.api, optionFix).then(res => res.json()).then(data => {\n        if (data.IsSuccess) {\n          this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Fixed);\n        } else {\n          this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsFailed, this.$_MResource.InformContent.Fixed);\n        }\n      });\n    },\n    /**\r\n    * Author:PXTRUONG(19/6/2023)\r\n    * Thực hiện đóng mở thông báo chuyền nội dung thông báo \r\n    */\n    showInform(isShow, informStatus, content) {\n      this.isShowInform = isShow;\n      this.informStatus = informStatus;\n      this.informContent = content;\n      setTimeout(() => this.isShowInform = false, 4000);\n    },\n    /**\r\n     * Author:PXTRUONG(31/5/2023)\r\n     * Thực hiện lưu và đóng form \r\n     */\n    async btnSave() {\n      // this.btnClose();\n      try {\n        this.isCheckValidate = true;\n        for (const inputRef in this.$refs) {\n          //tìm các input invalid\n          if (this.checkNull(this.$refs[inputRef].value)) {\n            this.inValue = true;\n            this.isShowAlert = true;\n            // console.log(this.$refs[inputRef].$refs.Minput.focus()); ; \n            this.alertContent = this.$refs[inputRef].inputContent + \" không được bỏ trống\";\n            return;\n          }\n        }\n\n        // console.log(this.$refs[\"PurchaseDate\"]);\n        if (this.formMode == this.$_MFormMode.FixAsset) {\n          this.fix();\n        } else if (this.formMode == this.$_MFormMode.AddAsset) {\n          this.add();\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(31/5/2023)\r\n     * Đống form và không lưu lại dữ liệu \r\n     */\n    btnCloseUnSave() {\n      if (this.formMode == this.$_MFormMode.FixAsset) {\n        this.statusAlert = this.$_MStatusAlert.IsFixAsset;\n        this.alertContent = null;\n        this.inValue = false;\n        this.isShowAlert = true;\n      } else if (this.formMode == this.$_MFormMode.AddAsset) {\n        this.statusAlert = this.$_MStatusAlert.IsAddFixAsset;\n        this.alertContent = null;\n        this.inValue = false;\n        this.isShowAlert = true;\n      }\n\n      // this.$emit(\"update:pelf\", this.pelftDefault);\n    },\n\n    /**\r\n     * \r\n     * Author:PXTRUONG(31/5/2023)\r\n     * thay đổi dữ liệu phụ thuộc khi thay đổi mã tài sản,loại tài sản \r\n     */\n    async bindData(api, value, protext) {\n      let option = {\n        method: 'GET',\n        headers: {\n          'Content-Type': 'application/json;charset=utf-8'\n        }\n      };\n      await fetch(api + \"?recodeCode=\" + value, option).then(res => res.json()).then(data => {\n        data.forEach(element => {\n          if (protext == \"DepartmentCode\") {\n            this.pelfInput.DepartmentName = element.DepartmentName;\n            this.pelfInput.DepartmentID = element.DepartmentID;\n          } else if (protext == \"FixedAssetCategoryCode\") {\n            this.pelfInput.FixedAssetCategoryName = element.FixedAssetCategoryName;\n            this.pelfInput.DepreciationRate = element.DepreciationRate;\n            this.pelfInput.LifeTime = element.LifeTime;\n            this.pelfInput.FixedAssetCategoryID = element.FixedAssetCategoryID;\n          }\n        });\n      });\n    }\n  },\n  data() {\n    return {\n      pelfInput: {},\n      pelftDefault: {},\n      listDepartment: [],\n      popupTitle: null,\n      atrophyYear: null,\n      department: [],\n      fixedAssetCategory: [],\n      data: {},\n      IsAllow: false,\n      isCheckValidate: false,\n      listInValue: [],\n      isShowAlert: false,\n      statusAlert: null,\n      alertContent: null,\n      inValue: false,\n      informStatus: null,\n      isShowInform: false,\n      informContent: null\n    };\n  }\n};","map":{"version":3,"names":["format","MAlert","pelfApi","Minput","MinputNumber","MDropdown","MDatapicker","MNotification","resource","maxlengthinput","components","props","pelf","formMode","created","now","Date","pelftDefault","JSON","stringify","parse","pelfInput","$_MFormMode","FixAsset","popupTitle","FormModeTitle","AddAsset","findMaxCode","TrackedYear","getFullYear","atrophyYear","Cost","DepreciationRate","mounted","$nextTick","$refs","FixedAssetCode","focus","methods","focusTop","event","keyCode","maxCode","fetch","fixedAsset","then","res","text","data","console","log","closeAlert","isForm","isShowAlert","inValue","inputRef","checkNull","value","inputContent","onfocus","btnClose","addListInValues","isPush","listInValue","push","filter","item","whiteSpace","$emit","add","option","method","headers","body","api","json","IsSuccess","$_MInformStatus","IsSucess","$_MResource","InformContent","Add","showInform","IsFailed","UserMessage","fix","optionFix","Fixed","isShow","informStatus","content","isShowInform","informContent","setTimeout","btnSave","isCheckValidate","alertContent","error","btnCloseUnSave","statusAlert","$_MStatusAlert","IsFixAsset","IsAddFixAsset","bindData","protext","forEach","element","DepartmentName","DepartmentID","FixedAssetCategoryName","LifeTime","FixedAssetCategoryID","listDepartment","department","fixedAssetCategory","IsAllow"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\view\\pelf\\PelfDetail.vue"],"sourcesContent":["<template>\r\n    <div class=\"body__popup\" id=\"detailEmployee\" tabindex=\"-1\" @keydown.ctrl.s=\"thu\">\r\n        <div class=\"popup\" ref=\"PeltForm\">\r\n            <div class=\"popup__body\">\r\n                <div class=\"popup__body__header\">\r\n                    <p class=\"popup__font--bold\">{{ popupTitle }}\r\n                    </p>\r\n                    <div class=\"icon popup__icon--cancal\" title=\"Đóng form\" @click=\"btnClose\"></div>\r\n                </div>\r\n\r\n                <div class=\"popup__body__container\">\r\n                    <div class=\"body__container\">\r\n                        <div class=\"popup__body__container--left\">\r\n\r\n                            <minput input-content=\"Mã tài sản\" :is-input-number=\"false\" :is-not-allow-null=\"true\"\r\n                                :is-nomal-input=\"true\" v-model=\"pelfInput.FixedAssetCode\"\r\n                                :is-check-validate=\"this.isCheckValidate\" ref=\"FixedAssetCode\"\r\n                                :setMaxLength=\"this.$_MMaxLengthInput.RecodeCode\">\r\n\r\n                            </minput>\r\n\r\n                        </div>\r\n                        <div class=\"popup__body__container--right\">\r\n                            <minput input-content=\"Tên tài sản \" :is-not-allow-null=\"true\"\r\n                                v-model=\"pelfInput.FixedAssetName\" :is-nomal-input=\"true\"\r\n                                :is-check-validate=\"this.isCheckValidate\" ref=\"FixedAssetName\"\r\n                                :setMaxLength=\"this.$_MMaxLengthInput.Nameother\">\r\n\r\n                            </minput>\r\n\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"body__container\">\r\n                        <div class=\"popup__body__container--left\">\r\n\r\n                            <m-dropdown input-content=\"Mã bộ phận sử dụng \" prop-text=\"DepartmentCode\"\r\n                                :is-not-allow-null=\"true\" :api=this.$_MApi.department.api v-model=\"pelfInput.DepartmentCode\"\r\n                                @sendData=\"bindData\" :is-check-validate=\"this.isCheckValidate\" ref=\"DepartmentCode\"\r\n                                :setMaxLength=\"this.$_MMaxLengthInput.RecodeCode\">\r\n\r\n                            </m-dropdown>\r\n\r\n\r\n                        </div>\r\n                        <div class=\"popup__body__container--right\">\r\n                            <minput input-content=\"Tên bộ phân sử dụng \" :is-input-number=\"false\" :is-disable=\"true\"\r\n                                v-model=\"pelfInput.DepartmentName\" :is-check-validate=\"this.isCheckValidate\"\r\n                                ref=\"DepartmentName\">\r\n\r\n                            </minput>\r\n\r\n\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"body__container\">\r\n                        <div class=\"popup__body__container--left\">\r\n\r\n                            <m-dropdown input-content=\" Mã loại tài sản \" :is-not-allow-null=\"true\"\r\n                                prop-text=\"FixedAssetCategoryCode\" :api=this.$_MApi.fixedAssetCategory.api\r\n                                v-model=\"pelfInput.FixedAssetCategoryCode\" @sendData=\"bindData\"\r\n                                :is-check-validate=\"this.isCheckValidate\" ref=\"FixedAssetCategoryCode\"\r\n                                :set-max-length=\"this.$_MMaxLengthInput.RecodeCode\">\r\n\r\n                            </m-dropdown>\r\n\r\n\r\n                        </div>\r\n                        <div class=\"popup__body__container--right\">\r\n                            <minput input-content=\"Tên loại tài sản \" :is-input-number=\"false\" :is-disable=\"true\"\r\n                                v-model=\"pelfInput.FixedAssetCategoryName\" :is-check-validate=\"this.isCheckValidate\"\r\n                                ref=\"FixedAssetCategoryName\">\r\n\r\n                            </minput>\r\n\r\n\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"body__container\">\r\n                        <div class=\"popup__body__container--left\">\r\n\r\n                            <minput-number input-content=\"Số lượng\" :is-input-number=\"true\" :is-not-allow-null=\"true\"\r\n                                :is-padding-right=\"true\" v-model=\"pelfInput.Quantity\"\r\n                                :is-check-validate=\"this.isCheckValidate\" ref=\"Quantity\"\r\n                                :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                            </minput-number>\r\n\r\n\r\n                        </div>\r\n                        <div class=\"popup__body__container--right\">\r\n                            <div class=\"input__container \">\r\n\r\n                                <div class=\"input__content--flex\">\r\n                                    <minput-number input-content=\"Nguyên giá\" :is-input-number=\"false\"\r\n                                        :is-not-allow-null=\"true\" :is-padding-right=\"true\" v-model=\"pelfInput.Cost\"\r\n                                        :is-check-validate=\"this.isCheckValidate\" ref=\"Cost\"\r\n                                        :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                                    </minput-number>\r\n\r\n                                    <minput-number input-content=\"Số năm sử dụng \" :is-input-number=\"false\"\r\n                                        :is-not-allow-null=\"true\" :is-padding-right=\"true\" v-model=\"pelfInput.LifeTime\"\r\n                                        :is-check-validate=\"this.isCheckValidate\" ref=\"LifeTime\"\r\n                                        :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                                    </minput-number>\r\n\r\n\r\n                                </div>\r\n\r\n                            </div>\r\n\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"body__container\">\r\n                        <div class=\"popup__body__container--left\">\r\n\r\n                            <minput-number input-content=\"Tỷ lệ hao mòn(%)\" :is-input-number=\"true\"\r\n                                :is-not-allow-null=\"true\" :is-padding-right=\"true\" v-model=\"pelfInput.DepreciationRate\"\r\n                                :is-check-validate=\"this.isCheckValidate\" ref=\"DepreciationRate\"\r\n                                :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                            </minput-number>\r\n\r\n\r\n\r\n                        </div>\r\n                        <div class=\"popup__body__container--right\">\r\n                            <div class=\"input__container \">\r\n\r\n                                <div class=\"input__content--flex\">\r\n                                    <minput-number input-content=\" Giá trị hao mòn năm \" :is-input-number=\"false\"\r\n                                        :is-not-allow-null=\"true\" :is-padding-right=\"true\" v-model=\"this.atrophyYear\"\r\n                                        :is-check-validate=\"this.isCheckValidate\" ref=\"atrophyYear\"\r\n                                        :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                                    </minput-number>\r\n\r\n                                    <minput input-content=\"Năm theo dõi \" :is-input-number=\"false\"\r\n                                        :is-disable-padding-right=\"true\" v-model=\"pelfInput.TrackedYear\"\r\n                                        :is-check-validate=\"this.isCheckValidate\" ref=\"TrackedYear\"\r\n                                        :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                                    </minput>\r\n\r\n\r\n                                </div>\r\n\r\n                            </div>\r\n\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"body__container\">\r\n                        <div class=\"popup__body__container--left\">\r\n\r\n                            <m-datapicker input-content=\"Ngày mua \" :is-not-allow-null=\"true\"\r\n                                v-model=\"pelfInput.PurchaseDate\" :is-check-validate=\"this.isCheckValidate\"\r\n                                ref=\"PurchaseDate\" :setMaxLength=\"this.$_MMaxLengthInput.number\"></m-datapicker>\r\n\r\n\r\n\r\n                        </div>\r\n                        <div class=\"popup__body__container--right\">\r\n                            <div class=\"input__container \">\r\n\r\n                                <div class=\"input__content--flex\">\r\n                                    <m-datapicker input-content=\"Ngày bắt đầu sử dụng \" :is-not-allow-null=\"true\"\r\n                                        v-model=\"pelfInput.DayUse\" :is-check-validate=\"this.isCheckValidate\"\r\n                                        ref=\"DayUse\"></m-datapicker>\r\n\r\n                                    <div class=\"input__container\"></div>\r\n\r\n                                </div>\r\n\r\n                            </div>\r\n\r\n                        </div>\r\n                    </div>\r\n\r\n\r\n                </div>\r\n\r\n\r\n            </div>\r\n\r\n            <div class=\"popup__pooter\">\r\n                <button class=\"button button--white\" @click=\"btnCloseUnSave\"> Hủy</button>\r\n                <button class=\"button button--blue\" @click=\"btnSave\" @keydown=\"focusTop\">Lưu</button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <m-alert v-if=\"isShowAlert\" :status-alert=\"this.statusAlert\" :list-delete=\"[]\" @close=\"closeAlert\"\r\n        :alert-content-input=\"this.alertContent\"></m-alert>\r\n    <m-notification :informStatus=\"this.informStatus\" v-if=\"this.isShowInform\"\r\n        :informContent=\"this.informContent\"></m-notification>\r\n</template>\r\n<script>\r\n\r\nimport format from '@/js/format/format';\r\nimport MAlert from '@/components/base/MAlert.vue';\r\nimport pelfApi from '@/config/pelf/pelfapi';\r\nimport Minput from \"@/components/base/Minput.vue\";\r\nimport MinputNumber from \"@/components/base/MInputNumber.vue\";\r\nimport MDropdown from \"@/components/base/MDropdown.vue\";\r\nimport MDatapicker from \"@/components/base/MDatapicker.vue\";\r\nimport MNotification from '@/components/base/MNotification.vue';\r\nimport resource from \"@/js/resource/resource\";\r\nimport maxlengthinput from \"@/js/enum/maxlengthinput.js\";\r\nexport default {\r\n    components: { Minput, MDropdown, MDatapicker, MinputNumber, MAlert, MNotification },\r\n    props: {\r\n        pelf: null,\r\n        formMode: null,\r\n\r\n\r\n    },\r\n    created() {\r\n\r\n        var now = new Date();\r\n        this.pelftDefault = JSON.stringify(this.pelf);\r\n        this.pelftDefault = JSON.parse(this.pelftDefault);\r\n        this.pelfInput = JSON.stringify(this.pelf);\r\n        this.pelfInput = JSON.parse(this.pelfInput);\r\n        if (this.formMode == this.$_MFormMode.FixAsset) {\r\n            this.popupTitle = resource.FormModeTitle.FixAsset\r\n\r\n        }\r\n        else if (this.formMode == this.$_MFormMode.AddAsset) {\r\n            this.popupTitle = resource.FormModeTitle.AddAsset\r\n            this.findMaxCode();\r\n            this.pelfInput.TrackedYear = now.getFullYear();\r\n        }\r\n\r\n        this.atrophyYear = this.pelfInput.Cost * this.pelfInput.DepreciationRate / 100;\r\n\r\n\r\n\r\n    },\r\n    mounted() {\r\n        //xét focus vào input mình muốn \r\n\r\n        this.$nextTick(function () {\r\n            this.$refs.FixedAssetCode.$refs.Minput.focus()\r\n        })\r\n\r\n\r\n\r\n\r\n    },\r\n\r\n    methods: {\r\n        /**\r\n         * Author:PXTRUONG(24/6/2023)\r\n         * Chuyển tab lên trang đầu \r\n         */\r\n        focusTop(event) {\r\n            if (event.keyCode == 9) {\r\n                // this.$nextTick(function () {\r\n                //     for (const inputRef in this.$refs) {\r\n                //         //tìm các input invalid\r\n                //         if (inputRef==\"FixedAssetCode\") {\r\n                //             this.$refs[inputRef].$refs.Minput.focus()\r\n                //             return;\r\n\r\n                //         }\r\n                //     }\r\n                    // this.$refs.FixedAssetCode.$refs.Minput.focus();\r\n            //     })\r\n            this.$refs[\"FixedAssetCode\"].$refs.Minput.focus();\r\n            return\r\n        }\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(22/6/2023)\r\n         * Tìm tìm FixedassetCode lớn nhất \r\n         */\r\n        async findMaxCode() {\r\n            var maxCode;\r\n            await fetch(pelfApi.fixedAsset.maxCode)\r\n                .then(res => res.text())\r\n                .then(data => {\r\n                    maxCode = data;\r\n                })\r\n            this.pelfInput.FixedAssetCode = maxCode;\r\n            console.log(maxCode);\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Đóng mở cảnh báo \r\n         */\r\n        closeAlert(isForm) {\r\n            if (isForm) {\r\n                this.isShowAlert = false;\r\n                return;\r\n            }\r\n            if (this.inValue) {\r\n                this.isShowAlert = false;\r\n                for (const inputRef in this.$refs) {\r\n\r\n                    //tìm các input invalid\r\n                    if (this.checkNull(this.$refs[inputRef].value)) {\r\n                        console.log(inputRef);\r\n                        console.log(this.$refs[inputRef].inputContent);\r\n                        this.$refs[inputRef].$refs.Minput.focus()\r\n                        console.log(this.$refs[inputRef].$refs.onfocus);\r\n                        console.log(this.$refs[inputRef].$refs);\r\n\r\n                        return;\r\n\r\n                    }\r\n                }\r\n            }\r\n            else {\r\n                this.isShowAlert = false;\r\n                this.btnClose();\r\n            }\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Add list invalid vào 1 mảng \r\n         */\r\n        addListInValues(data, isPush) {\r\n            if (isPush) {\r\n                this.listInValue.push(data);\r\n            }\r\n            else {\r\n                this.listInValue = this.listInValue.filter(item => item !== data)\r\n            }\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Check null\r\n         */\r\n        checkNull(data) {\r\n            data = format.whiteSpace(data)\r\n\r\n            if (!data) {\r\n\r\n                return true;\r\n            }\r\n            else {\r\n                return false;\r\n            }\r\n        },\r\n        /**\r\n         * đóng form chi tiết tài sản \r\n         * Author: PXTRUONG, Create: 24/3/2023\r\n         */\r\n        btnClose() {\r\n            this.$emit(\"hiddePolfDetail\");\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Thực hiện gọi api thêm tài sản \r\n         */\r\n        async add() {\r\n            let option = {\r\n                method: 'POST',\r\n                headers: {\r\n                    'Content-Type':\r\n                        'application/json;charset=utf-8'\r\n                },\r\n                body: JSON.stringify(this.pelfInput)\r\n            }\r\n            console.log(JSON.stringify(this.pelfInput));\r\n            await fetch(pelfApi.fixedAsset.api, option)\r\n                .then(res => res.json())\r\n                .then(data => {\r\n                    if (data.IsSuccess) {\r\n                        this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Add);\r\n                    }\r\n                    else {\r\n                        this.showInform(true, this.$_MInformStatus.IsFailed, data.UserMessage)\r\n                    }\r\n                })\r\n        },\r\n        /**\r\n        * Author:PXTRUONG(15/6/2023)\r\n        * Thực hiện gọi api sửa tài sản \r\n        */\r\n        async fix() {\r\n\r\n            let optionFix = {\r\n                method: 'PUT',\r\n                headers: {\r\n                    'Content-Type':\r\n                        'application/json;charset=utf-8'\r\n                },\r\n                body: JSON.stringify(this.pelfInput)\r\n            }\r\n            console.log(JSON.stringify(this.pelfInput));\r\n            await fetch(pelfApi.fixedAsset.api, optionFix)\r\n                .then(res => res.json())\r\n                .then(data => {\r\n                    if (data.IsSuccess) {\r\n                        this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Fixed);\r\n\r\n                    }\r\n                    else {\r\n                        this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsFailed, this.$_MResource.InformContent.Fixed);\r\n                    }\r\n                })\r\n\r\n        },\r\n        /**\r\n        * Author:PXTRUONG(19/6/2023)\r\n        * Thực hiện đóng mở thông báo chuyền nội dung thông báo \r\n        */\r\n        showInform(isShow, informStatus, content) {\r\n            this.isShowInform = isShow;\r\n            this.informStatus = informStatus;\r\n            this.informContent = content;\r\n            setTimeout(() => this.isShowInform = false, 4000);\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(31/5/2023)\r\n         * Thực hiện lưu và đóng form \r\n         */\r\n        async btnSave() {\r\n            // this.btnClose();\r\n            try {\r\n                this.isCheckValidate = true;\r\n                for (const inputRef in this.$refs) {\r\n\r\n                    //tìm các input invalid\r\n                    if (this.checkNull(this.$refs[inputRef].value)) {\r\n                        this.inValue = true;\r\n                        this.isShowAlert = true;\r\n                        // console.log(this.$refs[inputRef].$refs.Minput.focus()); ; \r\n                        this.alertContent = this.$refs[inputRef].inputContent + \" không được bỏ trống\";\r\n\r\n                        return;\r\n                    }\r\n                }\r\n\r\n                // console.log(this.$refs[\"PurchaseDate\"]);\r\n                if (this.formMode == this.$_MFormMode.FixAsset) {\r\n                    this.fix();\r\n                }\r\n                else if (this.formMode == this.$_MFormMode.AddAsset) {\r\n                    this.add();\r\n                }\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n\r\n\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(31/5/2023)\r\n         * Đống form và không lưu lại dữ liệu \r\n         */\r\n        btnCloseUnSave() {\r\n\r\n            if (this.formMode == this.$_MFormMode.FixAsset) {\r\n                this.statusAlert = this.$_MStatusAlert.IsFixAsset;\r\n                this.alertContent = null;\r\n                this.inValue = false;\r\n                this.isShowAlert = true;\r\n            }\r\n            else if (this.formMode == this.$_MFormMode.AddAsset) {\r\n                this.statusAlert = this.$_MStatusAlert.IsAddFixAsset;\r\n                this.alertContent = null;\r\n                this.inValue = false;\r\n                this.isShowAlert = true;\r\n            }\r\n\r\n\r\n            // this.$emit(\"update:pelf\", this.pelftDefault);\r\n\r\n\r\n        },\r\n        /**\r\n         * \r\n         * Author:PXTRUONG(31/5/2023)\r\n         * thay đổi dữ liệu phụ thuộc khi thay đổi mã tài sản,loại tài sản \r\n         */\r\n        async bindData(api, value, protext) {\r\n\r\n            let option = {\r\n                method: 'GET',\r\n                headers: {\r\n                    'Content-Type':\r\n                        'application/json;charset=utf-8'\r\n                },\r\n            };\r\n            await fetch(api + \"?recodeCode=\" + value, option)\r\n                .then((res) => res.json())\r\n                .then((data) => {\r\n                    data.forEach(element => {\r\n                        if (protext == \"DepartmentCode\") {\r\n                            this.pelfInput.DepartmentName = element.DepartmentName;\r\n                            this.pelfInput.DepartmentID = element.DepartmentID;\r\n                        }\r\n                        else if (protext == \"FixedAssetCategoryCode\") {\r\n                            this.pelfInput.FixedAssetCategoryName = element.FixedAssetCategoryName;\r\n                            this.pelfInput.DepreciationRate = element.DepreciationRate;\r\n                            this.pelfInput.LifeTime = element.LifeTime;\r\n                            this.pelfInput.FixedAssetCategoryID = element.FixedAssetCategoryID;\r\n                        }\r\n                    });\r\n\r\n                });\r\n\r\n\r\n        }\r\n\r\n    },\r\n    data() {\r\n        return {\r\n            pelfInput: {},\r\n            pelftDefault: {},\r\n            listDepartment: [],\r\n            popupTitle: null,\r\n            atrophyYear: null,\r\n            department: [],\r\n            fixedAssetCategory: [],\r\n            data: {},\r\n            IsAllow: false,\r\n            isCheckValidate: false,\r\n            listInValue: [],\r\n            isShowAlert: false,\r\n            statusAlert: null,\r\n            alertContent: null,\r\n            inValue: false,\r\n            informStatus: null,\r\n            isShowInform: false,\r\n            informContent: null\r\n        }\r\n    },\r\n}\r\n</script>\r\n<style>\r\n@import url(../../../style/base/popup.css);\r\n@import url(../../../style/base/textfield.css);\r\n@import url(../../../style/base/icon.css);\r\n</style>"],"mappings":";AAsMA,OAAOA,MAAK,MAAO,oBAAoB;AACvC,OAAOC,MAAK,MAAO,8BAA8B;AACjD,OAAOC,OAAM,MAAO,uBAAuB;AAC3C,OAAOC,MAAK,MAAO,8BAA8B;AACjD,OAAOC,YAAW,MAAO,oCAAoC;AAC7D,OAAOC,SAAQ,MAAO,iCAAiC;AACvD,OAAOC,WAAU,MAAO,mCAAmC;AAC3D,OAAOC,aAAY,MAAO,qCAAqC;AAC/D,OAAOC,QAAO,MAAO,wBAAwB;AAC7C,OAAOC,cAAa,MAAO,6BAA6B;AACxD,eAAe;EACXC,UAAU,EAAE;IAAEP,MAAM;IAAEE,SAAS;IAAEC,WAAW;IAAEF,YAAY;IAAEH,MAAM;IAAEM;EAAc,CAAC;EACnFI,KAAK,EAAE;IACHC,IAAI,EAAE,IAAI;IACVC,QAAQ,EAAE;EAGd,CAAC;EACDC,OAAOA,CAAA,EAAG;IAEN,IAAIC,GAAE,GAAI,IAAIC,IAAI,CAAC,CAAC;IACpB,IAAI,CAACC,YAAW,GAAIC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACP,IAAI,CAAC;IAC7C,IAAI,CAACK,YAAW,GAAIC,IAAI,CAACE,KAAK,CAAC,IAAI,CAACH,YAAY,CAAC;IACjD,IAAI,CAACI,SAAQ,GAAIH,IAAI,CAACC,SAAS,CAAC,IAAI,CAACP,IAAI,CAAC;IAC1C,IAAI,CAACS,SAAQ,GAAIH,IAAI,CAACE,KAAK,CAAC,IAAI,CAACC,SAAS,CAAC;IAC3C,IAAI,IAAI,CAACR,QAAO,IAAK,IAAI,CAACS,WAAW,CAACC,QAAQ,EAAE;MAC5C,IAAI,CAACC,UAAS,GAAIhB,QAAQ,CAACiB,aAAa,CAACF,QAAO;IAEpD,OACK,IAAI,IAAI,CAACV,QAAO,IAAK,IAAI,CAACS,WAAW,CAACI,QAAQ,EAAE;MACjD,IAAI,CAACF,UAAS,GAAIhB,QAAQ,CAACiB,aAAa,CAACC,QAAO;MAChD,IAAI,CAACC,WAAW,CA