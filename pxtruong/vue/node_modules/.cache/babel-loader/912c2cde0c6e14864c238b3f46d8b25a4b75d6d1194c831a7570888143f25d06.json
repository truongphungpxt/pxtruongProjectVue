t;\n      setTimeout(() => this.isShowInform = false, 4000);\n    },\n    /**\r\n      * hiện chi tiết tài sản \r\n      * Author:PXTRUONG(24/05/2023)\r\n      */\n    showPelfDetail(data, formMode) {\n      this.fromMode = formMode;\n      this.pelfDetail = data;\n      this.isShow = true;\n    },\n    /**\r\n     * đóng chi tiết tài sản \r\n     * Author:PXTRUONG(24/05/2023)\r\n     */\n    closePelfDetail(showInform, result, content) {\n      if (showInform) {\n        this.showInform(showInform, result, content);\n      }\n      this.isShow = false;\n    },\n    /**\r\n     * Author:PXTRUONG(6/6/2023)\r\n     * thực hiện xóa nhiều \r\n     */\n    showDeleteMuntiple() {\n      this.isShowAlert = true;\n      console.log(this.listDelete);\n    }\n  },\n  data() {\n    return {\n      dataInput: [],\n      isShow: false,\n      pelfDetail: null,\n      isResize: false,\n      listDelete: [],\n      isShowAlert: false,\n      fromMode: null,\n      informStatus: null,\n      isShowInform: false,\n      informContent: null,\n      isDeleteStatus: Boolean\n    };\n  }\n};","map":{"version":3,"names":["PelfDetail","ListPelf","MDropdown","MAlert","MNotification","components","created","$eventBus","on","data","isResize","listDelete","methods","closeAlert","isAllowDelete","listData","forEach","element","push","JSON","stringify","console","log","fetch","pelfApi","fixedAsset","api","method","then","res","json","IsSuccess","isDeleteStatus","isShowAlert","showInform","$_MInformStatus","IsSucess","$_MResource","InformContent","DeleteSucess","error","isShow","informStatus","content","isShowInform","informContent","setTimeout","showPelfDetail","formMode","fromMode","pelfDetail","closePelfDetail","result","showDeleteMuntiple","dataInput","Boolean"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\view\\pelf\\PelfBody.vue"],"sourcesContent":["<template>\r\n    <div class=\"body\" :class=\"{ 'resize__body': isResize }\"