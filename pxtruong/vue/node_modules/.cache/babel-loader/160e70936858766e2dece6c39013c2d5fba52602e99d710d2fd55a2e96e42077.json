/maxlengthinput.js\";\r\nexport default {\r\n    components: { Minput, MDropdown, MDatapicker, MinputNumber, MAlert ,MNotification},\r\n    props: {\r\n        pelf: null,\r\n        formMode: null,\r\n\r\n\r\n    },\r\n    created() {\r\n        var now= new Date();\r\n        this.pelftDefault = JSON.stringify(this.pelf);\r\n        this.pelftDefault = JSON.parse(this.pelftDefault);\r\n        this.pelfInput = JSON.stringify(this.pelf);\r\n        this.pelfInput = JSON.parse(this.pelfInput);\r\n        if (this.formMode == this.$_MFormMode.FixAsset) {\r\n            this.popupTitle = resource.FormModeTitle.FixAsset\r\n            \r\n        }\r\n        else if (this.formMode == this.$_MFormMode.AddAsset) {\r\n            this.popupTitle = resource.FormModeTitle.AddAsset\r\n            this.pelfInput.TrackedYear =  now.getFullYear();\r\n        }\r\n        console.log(this.pelf);\r\n        console.log(this.formMode);\r\n        this.atrophyYear = this.pelfInput.Cost * this.pelfInput.DepreciationRate / 100;\r\n\r\n\r\n\r\n    },\r\n    methods: {\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Đóng mở cảnh báo \r\n         */\r\n        closeAlert(isForm) {\r\n            if (isForm) {\r\n                this.isShowAlert = false;\r\n                return;\r\n            }\r\n            if (this.inValue) {\r\n                this.isShowAlert = false;\r\n            }\r\n            else {\r\n                this.isShowAlert = false;\r\n                this.btnClose();\r\n            }\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Add list invalid vào 1 mảng \r\n         */\r\n        addListInValues(data, isPush) {\r\n            if (isPush) {\r\n                this.listInValue.push(data);\r\n            }\r\n            else {\r\n                this.listInValue = this.listInValue.filter(item => item !== data)\r\n            }\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Check null\r\n         */\r\n        checkNull(data) {\r\n            data=format.whiteSpace(data)\r\n        \r\n        if (!data) {\r\n            \r\n            return true;\r\n        }\r\n        else {\r\n            return false;\r\n        }\r\n        },\r\n        /**\r\n         * đóng form chi tiết tài sản \r\n         * Author: PXTRUONG, Create: 24/3/2023\r\n         */\r\n        btnClose() {\r\n            this.$emit(\"hiddePolfDetail\");\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Thực hiện gọi api thêm tài sản \r\n         */\r\n        async add() {\r\n            let option = {\r\n                method: 'POST',\r\n                headers: {\r\n                    'Content-Type':\r\n                        'application/json;charset=utf-8'\r\n                },\r\n                body: JSON.stringify(this.pelfInput)\r\n            }\r\n            console.log(JSON.stringify(this.pelfInput));\r\n            await fetch(pelfApi.fixedAsset.api, option)\r\n                .then(res => res.json())\r\n                .then(data => {\r\n                    if (data.IsSuccess) {\r\n                        this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Add);\r\n                    }\r\n                else{\r\n                        this.showInform(true, this.$_MInformStatus.IsFailed,data.UserMessage)\r\n                    }\r\n                })\r\n        },\r\n        /**\r\n        * Author:PXTRUONG(15/6/2023)\r\n        * Thực hiện gọi api sửa tài sản \r\n        */\r\n        async fix() {\r\n           \r\n            let optionFix = {\r\n                method: 'PUT',\r\n                headers: {\r\n                    'Content-Type':\r\n                        'application/json;charset=utf-8'\r\n                },\r\n                body: JSON.stringify(this.pelfInput)\r\n            }\r\n            console.log(JSON.stringify(this.pelfInput));\r\n            await fetch(pelfApi.fixedAsset.api, optionFix)\r\n                .then(res => res.json())\r\n                .then(data => {\r\n                    if (data.IsSuccess) {\r\n                        this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Fixed);\r\n                        \r\n                    }\r\n                    else {\r\n                        this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsFailed, this.$_MResource.InformContent.Fixed);\r\n                    }\r\n                })\r\n\r\n        },\r\n         /**\r\n         * Author:PXTRUONG(19/6/2023)\r\n         * Thực hiện đóng mở thông báo chuyền nội dung thông báo \r\n         */\r\n         showInform(isShow, informStatus,content ) {\r\n            this.isShowInform = isShow;\r\n            this.informStatus = informStatus;\r\n            this.informContent = content;\r\n            setTimeout(() => this.isShowInform = false, 4000);\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(31/5/2023)\r\n         * Thực hiện lưu và đóng form \r\n         */\r\n        async btnSave() {\r\n            // this.btnClose();\r\n            try {\r\n                this.isCheckValidate = true;\r\n                for (const inputRef in this.$refs) {\r\n                    //tìm các input invalid\r\n                    if (this.checkNull(this.$refs[inputRef].value)) {\r\n                        console.log(this.$refs[inputRef].inputContent);\r\n                        this.inValue = true;\r\n                        this.isShowAlert = true;\r\n                        this.alertContent = this.$refs[inputRef].inputContent + \" không được bỏ trống\";\r\n\r\n                        return;\r\n                    }\r\n                }\r\n\r\n                // console.log(this.$refs[\"PurchaseDate\"]);\r\n                if (this.formMode == this.$_MFormMode.FixAsset) {\r\n                    this.fix();\r\n                }\r\n                else if (this.formMode == this.$_MFormMode.AddAsset) {\r\n                    this.add();\r\n                }\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n\r\n\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(31/5/2023)\r\n         * Đống form và không lưu lại dữ liệu \r\n         */\r\n        btnCloseUnSave() {\r\n\r\n            if (this.formMode == this.$_MFormMode.FixAsset) {\r\n                this.statusAlert = this.$_MStatusAlert.IsFixAsset;\r\n                this.alertContent = null;\r\n                this.inValue = false;\r\n                this.isShowAlert = true;\r\n            }\r\n            else if (this.formMode == this.$_MFormMode.AddAsset) {\r\n                this.statusAlert = this.$_MStatusAlert.IsAddFixAsset;\r\n                this.alertContent = null;\r\n                this.inValue = false;\r\n                this.isShowAlert = true;\r\n            }\r\n\r\n\r\n            // this.$emit(\"update:pelf\", this.pelftDefault);\r\n\r\n\r\n        },\r\n        /**\r\n         * \r\n         * Author:PXTRUONG(31/5/2023)\r\n         * thay đổi dữ liệu phụ thuộc khi thay đổi mã tài sản,loại tài sản \r\n         */\r\n        async bindData(api, value, protext) {\r\n\r\n            let option = {\r\n                method: 'GET',\r\n                headers: {\r\n                    'Content-Type':\r\n                        'application/json;charset=utf-8'\r\n                },\r\n            };\r\n            await fetch(api + \"?recodeCode=\" + value, option)\r\n                .then((res) => res.json())\r\n                .then((data) => {\r\n                    data.forEach(element => {\r\n                        if (protext == \"DepartmentCode\") {\r\n                            this.pelfInput.DepartmentName = element.DepartmentName;\r\n                            this.pelfInput.DepartmentID = element.DepartmentID;\r\n                        }\r\n                        else if (protext == \"FixedAssetCategoryCode\") {\r\n                            this.pelfInput.FixedAssetCategoryName = element.FixedAssetCategoryName;\r\n                            this.pelfInput.DepreciationRate = element.DepreciationRate;\r\n                            this.pelfInput.LifeTime = element.LifeTime;\r\n                            this.pelfInput.FixedAssetCategoryID = element.FixedAssetCategoryID;\r\n                        }\r\n                    });\r\n\r\n                });\r\n\r\n\r\n        }\r\n\r\n    },\r\n    data() {\r\n        return {\r\n            pelfInput: {},\r\n            pelftDefault: {},\r\n            listDepartment: [],\r\n            popupTitle: null,\r\n            atrophyYear: null,\r\n            department: [],\r\n            fixedAssetCategory: [],\r\n            data: {},\r\n            IsAllow: false,\r\n            isCheckValidate: false,\r\n            listInValue: [],\r\n            isShowAlert: false,\r\n            statusAlert: null,\r\n            alertContent: null,\r\n            inValue: false,\r\n            informStatus: null,\r\n            isShowInform: false,\r\n            informContent: null\r\n        }\r\n    },\r\n}\r\n</script>\r\n<style>\r\n@import url(../../../style/base/popup.css);\r\n@import url(../../../style/base/textfield.css);\r\n@import url(../../../style/base/icon.css);\r\n</style>"],"mappings":";;EAEaA,KAAK,EAAC;AAAO;;EACTA,KAAK,EAAC;AAAa;;EACfA,KAAK,EAAC;AAAqB;;EACzBA,KAAK,EAAC;AAAmB;;EAK3BA,KAAK,EAAC;AAAwB;;EAC1BA,KAAK,EAAC;AAA8B;;EASpCA,KAAK,EAAC;AAA+B;;EAa7CA,KAAK,EAAC;AAAe;;;;;6DAhClCC,mBAAA,CAqCM;IArCDD,KAAK,EAAC,aAAa;IAACE,EAAE,EAAC,gBAAgB;IAACC,QAAQ,EAAC,IAAI;IAAEC,SAAO,EAAAC,MAAA,QAAAA,MAAA,MAAAC,SAAA,CAAAC,cAAA,KAAAC,IAAA,KAASC,IAAA,CAAAC,GAAA,IAAAD,IAAA,CAAAC,GAAA,IAAAF,IAAA,CAAG;MAC3EP,mBAAA,CAmCM,OAnCNU,UAmCM,GAlCFV,mBAAA,CA4BM,OA5BNW,UA4BM,GA3BFX,mBAAA,CAIM,OAJNY,UAIM,GAHFZ,mBAAA,CACI,KADJa,UACI,EAAAC,gBAAA,CAD4BC,KAAA,CAAAC,UAAU,kBAE1ChB,mBAAA,CAAgF;