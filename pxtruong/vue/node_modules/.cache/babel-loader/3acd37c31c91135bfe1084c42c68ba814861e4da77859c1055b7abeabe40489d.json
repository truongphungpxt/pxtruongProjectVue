\r\n            else if(this.listDelete.length == 1) {\r\n                this.findFixedName();\r\n                console.log(this.alertContent);\r\n            }\r\n            else{\r\n                this.btnWhite = null;\r\n                this.btnBlue = this.$_MResource.buttonTitle.Close;\r\n                this.alertContent = this.$_MResource.AlertContent.Undefine ;\r\n            }\r\n        }\r\n        if(this.statusAlert==this.$_MStatusAlert.IsAddFixAsset){\r\n                this.btnWhite = this.$_MResource.buttonTitle.No;\r\n                this.btnBlue = this.$_MResource.buttonTitle.Cancal;\r\n                this.alertContent = this.$_MResource.AlertContent.IsCancal ;\r\n        }\r\n        if(this.statusAlert==this.$_MStatusAlert.IsFixAsset){\r\n                this.btnWhite = this.$_MResource.buttonTitle.Cancal;\r\n                this.btnBlue = this.$_MResource.buttonTitle.Save;\r\n                this.btnBorderBlue=this.$_MResource.buttonTitle.UnSave;\r\n                this.alertContent = this.$_MResource.AlertContent.UnSave ;\r\n        }\r\n        if(this.alertContentInput){\r\n            this.alertContent=this.alertContentInput;\r\n            this.btnWhite=false;\r\n            this.btnBlue = this.$_MResource.buttonTitle.Close;\r\n        }\r\n      \r\n\r\n    },\r\n    methods: {\r\n        /**\r\n         * Author:PXTRUONG(21/6/2013)\r\n         * Thực hiện tìm tên tài sản \r\n         */\r\n     async   findFixedName(){\r\n              await  fetch(pelfApi.fixedAsset.api+\"?recodeCode=\"+this.listDelete[0])\r\n                .then((res) => res.json())\r\n                .then((value) => {\r\n                    this.alertContent = this.$_MResource.AlertContent.Delete +  value[0].FixedAssetName;\r\n                        console.log( this.alertContent );\r\n                });\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(6/6/2023)\r\n         * Thực hiện các lên và đóng form \r\n         */\r\n        btnActive(){\r\n            this.closeAlert();\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(6/6/2023)\r\n         * Thực hiện  đóng form \r\n         */\r\n        closeAlert(){\r\n            if(this.listDelete.length>0){\r\n                this.$emit(\"close\",true);\r\n            }\r\n            else{\r\n                this.$emit(\"close\");\r\n            }\r\n           \r\n        },\r\n        /**\r\n         * Author:PXTRUONG(19/6/2023)\r\n         * Thực hiện đóng cảnh báo khi nhân nút hủy \r\n         */\r\n        close(){\r\n          \r\n            if(this.statusAlert==this.$_MStatusAlert.IsAddFixAsset||this.statusAlert==this.$_MStatusAlert.IsFixAsset){\r\n                this.$emit(\"close\",true);\r\n            }\r\n            else{\r\n                this.$emit(\"close\");\r\n            }\r\n\r\n        }\r\n\r\n    },\r\n    data() {\r\n        return {\r\n            btnWhite: null,\r\n            btnBlue: null,\r\n            btnBorderBlue: null,\r\n            alertContent: null,\r\n            assetName:null\r\n          \r\n        }\r\n    },\r\n}\r\n</script>\r\n<style scoped>\r\n@import url(../../style/base/popup.css);\r\n@import url(../../style/base/icon.css);\r\n@import url(../../style/base/button.css);\r\n@import url(../../style/base/alert.css);\r\n</style>"],"mappings":"AAuBA,OAAOA,OAAM,MAAO,uBAAuB;AAC3C,eAAe;EACXC,KAAK,EAAE;IACHC,QAAQ,EAAG;MACPC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACb,CAAC;IACDC,iBAAiB,EAAC,IAAI;IACtBC,YAAY,EAAE;MACVJ,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACb,CAAC;IACDG,eAAe,EAAC;MACZL,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACb,CAAC;IACDI,WAAW,EAAC,IAAI;IAChBC,UAAU,EAAC,EAAE;IACbC,SAAS,EAACP;EAGd,CAAC;EAEDQ,OAAOA,CAAA,EAAG;IAGJ;IACA,IAAI,IAAI,CAACV,QAAQ,EAAE;MACjB,IAAI,CAACW,QAAO,GAAI,IAAI,CAACC,WAAW,CAACC,WAAW,CAACC,EAAE;MAC/C,IAAI,CAACC,OAAM,GAAI,IAAI,CAACH,WAAW,CAACC,WAAW,CAACG,MAAM;MAClD,IAAI,IAAI,CAACR,UAAU,CAACS,MAAK,GAAI,CAAC,EAAE;QAC5B,IAAI,CAACC,YAAW,GAAO,IAAI,CAACN,WAAW,CAACO,YAAY,CAACC,cAAc;MACvE,OACK,IAAG,IAAI,CAACZ,U