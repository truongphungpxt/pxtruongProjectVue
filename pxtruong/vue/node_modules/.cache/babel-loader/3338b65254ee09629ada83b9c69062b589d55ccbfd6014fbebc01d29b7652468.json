{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport pelfApi from '@/config/pelf/pelfapi';\nimport format from '@/js/format/format';\nimport resource from '@/js/resource/resource';\nimport MCheckBox from '@/components/base/MCheckBox.vue';\nimport MDropdown from '@/components/base/MDropdown.vue';\nimport MPaging from '@/components/base/MPaging.vue';\nimport MContextMenu from '@/components/base/MContextMenu.vue';\nexport default {\n  components: {\n    MCheckBox,\n    MDropdown,\n    MPaging,\n    MContextMenu\n  },\n  created() {\n    this.loadData();\n    this.$eventBus.on(\"contextMenuStatus\", data => {\n      this.contextMenuStatus = data;\n    });\n  },\n  watch: {\n    contextMenuStatus: function (newValue) {\n      this.actionContextMenu(newValue);\n    },\n    listChecked: function (newValue) {\n      this.$eventBus.emit(\"listCodeDelete\", newValue);\n    },\n    isDeleteSucess: function (newValue) {\n      if (newValue == true) {\n        this.listChecked.forEach(element => {\n          this.listData.forEach(elementData => {\n            if (element == elementData.FixedAssetCode) {\n              this.listData.splice(this.data.indexOf(elementData), 1);\n            }\n          });\n        });\n      }\n      console.log(this.listData);\n      this.$emit(\"restartDeleteStatus\");\n    },\n    listData: function (newValue) {\n      if (newValue.length > 0) {\n        this.isNull = false;\n      } else {\n        this.isNull = true;\n      }\n    }\n  },\n  props: {\n    isDeleteSucess: Boolean\n  },\n  methods: {\n    /**\r\n     * Author:PXTRUONG(25/6/2023)\r\n     * Thực hiện chức năng của context theo trạng thái action\r\n     * @param {trạng thái muốn thực hiên:thêm,sửa.xóa} status \r\n     */\n    actionContextMenu(status) {\n      switch (status) {\n        // case this.$_MContextStatus.IsAdd:\n        //     console.log(\"thêm\");\n        //     break;\n        case this.$_MContextStatus.IsDelete:\n          this.listChecked = [];\n          this.listChecked.push(this.peltSellected.FixedAssetCode);\n          this.$eventBus.emit(\"listCodeDelete\", this.listChecked);\n          this.$eventBus.emit(\"contextMenuStatus\", null);\n          break;\n        case this.$_MContextStatus.IsFixed:\n          this.$emit(\"showDetail\", this.peltSellected, this.$_MFormMode.FixAsset);\n          this.$eventBus.emit(\"contextMenuStatus\", null);\n          break;\n        case this.$_MContextStatus.IsDuplicate:\n          this.$emit(\"showDetail\", this.peltSellected, this.$_MFormMode.AddAsset);\n          this.$eventBus.emit(\"contextMenuStatus\", null);\n          break;\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(25/6/2023)\r\n     * Đóng mở contextMenu\r\n     */\n    showContextMenu(item) {\n      window.oncontextmenu = function (e) {\n        // Cancel the event\n        e.preventDefault();\n      };\n      if (!this.isShowMenuContext) {\n        console.log(item);\n        this.peltSellected = item;\n        this.pageX = event.pageX;\n        this.pageY = event.pageY + 15;\n        console.log(\"x\", event.pageX);\n        console.log(\"y\", event.pageY);\n        this.isShowMenuContext = true;\n      } else {\n        this.isShowMenuContext = false;\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(25/6/2023)\r\n     * Nhấn ctrl để chỉ chọn 1 đối tượng\r\n     * @param {giá trị mã } data \r\n     * @param {vị trí của mã được chọn} index \r\n     */\n    oneSellected(data, index) {\n      this.firstSellect = index;\n      let item = [];\n      item.push(data);\n      this.listChecked = item;\n    },\n    /**\r\n     * Author:PXTRUONG(25/6/2023)\r\n     * Nhấn shift để chọn nhiều đối tượng \r\n     * @param {giá trị mã } data \r\n     * @param {vị trí của mã được chọn} index \r\n     */\n    moveSellected(data, index) {\n      var lastSelect = index + 1;\n      var list = [];\n      if (lastSelect < this.firstSellect) {\n        list = this.listData.slice(lastSelect - 1, this.firstSellect + 1);\n      } else {\n        list = this.listData.slice(this.firstSellect, lastSelect);\n      }\n      let listDelete = [];\n      list.forEach(element => {\n        listDelete.push(element.FixedAssetCode);\n      });\n      this.listChecked = listDelete;\n      console.log(this.listChecked);\n    },\n    /**\r\n     * Author:PXTRUONG(24/6/2023)\r\n     * tìm các mã nhân viên có trong 1 trang \r\n     */\n    findFixedAssetCode() {\n      var data = [];\n      this.listData.forEach(element => {\n        data.push(element.FixedAssetCode);\n      });\n      console.log(data);\n      return data;\n    },\n    /**\r\n     * Author:PXTRUONG(22/6/2023\r\n     * Thực hiện check tất cả \r\n     */\n    checkAll() {\n      if (this.ischeckall == true) {\n        // var listDelete = [];\n        // this.listData.forEach(element => {\n        //     listDelete.push(element.FixedAssetCode)\n\n        // });\n        // this.listChecked = listDelete\n        this.listChecked = this.listChecked.filter(item => !this.findFixedAssetCode().includes(item));\n        console.log(\"uncheck\", this.listChecked);\n      } else {\n        this.listChecked = this.findFixedAssetCode();\n        console.log(this.listChecked);\n      }\n\n      // return listChecked;\n    },\n\n    /**\r\n     * Author:PXTRUONG(21/6/2023)\r\n     * Chuyền dữ liệu sau khi phan trang \r\n     */\n    bindDate(data) {\n      this.listData = data;\n    },\n    /**\r\n    * Load dữ liệu từ api cho vào bảng\r\n    * Author:Phùng Xuân Trường\r\n    * 24/2/2023\r\n    */\n    async loadData() {\n      try {\n        await fetch(pelfApi.fixedAsset.getPaging + '?PageSize= 10' + '&PageNumber= 1').then(res => res.json()).then(value => {\n          this.listData = value.data;\n          this.totalRecord = value.data[0].TotalRecord;\n          ;\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Kiểm tra mã có trong list được tích hay không\r\n     */\n    chekedList(data, index) {\n      if (this.listChecked.includes(data)) {\n        return true;\n      } else if (index == this.indexSelected) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Đổi màu khi click chọn\r\n     */\n    selectItem(index, data) {\n      this.isShowMenuContext = false;\n      let item = [];\n      item.push(data);\n      if (this.listChecked.length == 0) {\n        this.firstSellect = index;\n        this.listChecked.push(data);\n      } else if (this.listChecked.length == 1 && !this.listChecked.includes(data)) {\n        this.listChecked = item;\n      }\n      this.$eventBus.emit(\"listCodeDelete\", this.listChecked);\n    },\n    /**\r\n     * Author:PXTRUONG, Create: 27/5/2023\r\n     * gọi hàm format tiền\r\n     */\n    formatMoney(money) {\n      return format.fomatmoney(money);\n    },\n    /**\r\n     * Author:PXTRUONG, Create: 27/5/2023\r\n     * hiện sửa \r\n     */\n    showPeltDetail(data) {\n      console.log(data);\n      this.$emit(\"showDetail\", data, this.$_MFormMode.FixAsset);\n    }\n  },\n  data() {\n    return {\n      peltSellected: null,\n      data: [],\n      tableLabel: resource.tableTitle,\n      listChecked: [],\n      listData: [],\n      indexSelected: null,\n      renderComponent: true,\n      isNull: false,\n      listPage: [{\n        \"PageNumber\": \"20\"\n      }, {\n        \"PageNumber\": \"30\"\n      }, {\n        \"PageNumber\": \"40\"\n      }, {\n        \"PageNumber\": \"50\"\n      }],\n      setWidth: [],\n      totalRecord: null,\n      keyWordInput: null,\n      startXMouse: null,\n      ischeckall: false,\n      firstSellect: null,\n      pageX: null,\n      pageY: null,\n      isShowMenuContext: false,\n      contextMenuStatus: null\n    };\n  }\n};","map":{"version":3,"names":["pelfApi","format","resource","MCheckBox","MDropdown","MPaging","MContextMenu","components","created","loadData","$eventBus","on","data","contextMenuStatus","watch","newValue","actionContextMenu","listChecked","emit","isDeleteSucess","forEach","element","listData","elementData","FixedAssetCode","splice","indexOf","console","log","$emit","length","isNull","props","Boolean","methods","status","$_MContextStatus","IsDelete","push","peltSellected","IsFixed","$_MFormMode","FixAsset","IsDuplicate","AddAsset","showContextMenu","item","window","oncontextmenu","e","preventDefault","isShowMenuContext","pageX","event","pageY","oneSellected","index","firstSellect","moveSellected","lastSelect","list","slice","listDelete","findFixedAssetCode","checkAll","ischeckall","filter","includes","bindDate","fetch","fixedAsset","getPaging","then","res","json","value","totalRecord","TotalRecord","error","chekedList","indexSelected","selectItem","formatMoney","money","fomatmoney","showPeltDetail","tableLabel","tableTitle","renderComponent","listPage","setWidth","keyWordInput","startXMouse"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\view\\pelf\\ListPelf.vue"],"sourcesContent":["<template>\r\n    <div class=\"body__content\" v-if=\"this.renderComponent == true\">\r\n\r\n        <table class=\"table__content\">\r\n            <thead class=\"table__header\">\r\n                <th class=\"  th__checkbox table__Maxwidth\">\r\n                    <input type=\"checkbox\" class=\"input__checkbox input__checkbox--icon\" v-model=\"ischeckall\"\r\n                        @click=\"checkAll\">\r\n                </th>\r\n                <!-- <th title=\"Số thứ tự\" class=\"table__Maxwidth\"   \r\n                >\r\n                    {{ this.$_MResource.tableTitle.STT.value }}\r\n                </th>\r\n                <th class=\"table__Minwidth\">\r\n                    {{ this.$_MResource.tableTitle.FixedAssetCode.value }}\r\n                </th>\r\n                <th class=\"table__Minwidth\">\r\n                    {{ this.$_MResource.tableTitle.FixedAssetName.value }}\r\n                </th>\r\n                <th class=\"table__Minwidth\">\r\n                    {{ this.$_MResource.tableTitle.FixedAssetCategoryName.value }}\r\n                </th>\r\n                <th class=\"table__Minwidth\" >\r\n                    {{ this.$_MResource.tableTitle.DepartmentName.value }}\r\n                </th>\r\n                <th class=\"text--right \">\r\n                    {{ this.$_MResource.tableTitle.Quantity.value }}\r\n                </th>\r\n\r\n                <th class=\"text--right\">\r\n                    {{ this.$_MResource.tableTitle.Cost.value }}\r\n                </th>\r\n                <th title=\" Hao mòn/ khấu hao luỹ kế\" class=\"text--right\">\r\n                    {{ this.$_MResource.tableTitle.Accumulated.value }}\r\n                </th>\r\n                <th class=\"text--right\">\r\n                    {{ this.$_MResource.tableTitle.ResidualValue.value }}\r\n                </th>\r\n                <th>\r\n                    {{ this.$_MResource.tableTitle.Action.value }}\r\n                </th> -->\r\n                <th v-for=\"(item) in this.$_MResource.tableTitle \" :key=\"item\"\r\n                    :class=\"{ 'text--right': item.type == 'number', 'text--center': item.value == this.$_MResource.tableTitle.STT.value }\">\r\n                    {{ item.value }}\r\n\r\n\r\n                    <!-- <div class=\"resize\"></div> -->\r\n\r\n\r\n                </th>\r\n\r\n            </thead>\r\n\r\n            <tbody v-if=\"this.renderComponent\">\r\n                <!-- <div v-if=\"this.renderComponent\"> -->\r\n                <tr v-for=\"(item, index) in listData\" :key=\"item\"\r\n                    :class=\"{ 'table__bgr--sellect': this.chekedList(item.FixedAssetCode, index) }\"\r\n                    @mousedown.shift=\"moveSellected(item.FixedAssetCode, index)\"\r\n                    @mousedown.ctrl=\"oneSellected(item.FixedAssetCode, index)\">\r\n\r\n                    <td>\r\n                        <input class=\" input__checkbox input__checkbox--icon\" type=\"checkbox\" v-model=\"listChecked\"\r\n                            :value=\"item.FixedAssetCode\">\r\n\r\n                    </td>\r\n                    <td @click=\"selectItem(index, item.FixedAssetCode)\" class=\"text--center\">\r\n                        {{ index + 1 }}\r\n                    </td>\r\n                    <td v-for=\"(itemindex) in this.$_MResource.assetTitle \" :key=\"itemindex\"\r\n                        @click=\"selectItem(index, item.FixedAssetCode)\" @mousedown.right=\"showContextMenu(item)\"\r\n                        :class=\"{ 'text--right': itemindex.type == 'number' }\" :style=\"[`width:${this.setWidth[index]}`]\">\r\n                        {{\r\n                            item[itemindex.key]\r\n\r\n                        }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\">\r\n                        {{ this.formatMoney(1200000) }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\">\r\n                        {{ this.formatMoney(item.Cost - 1200000) }}\r\n                    </td>\r\n                    <!-- <td @click=\"selectItem(index, item.FixedAssetCode)\" class=\"text--center\" >\r\n                        {{ index + 1 }}\r\n                    </td>\r\n                    <td @click=\"selectItem(index, item.FixedAssetCode)\" >\r\n                        {{ item.FixedAssetCode }}\r\n                    </td>\r\n                    <td @click=\"selectItem(index, item.FixedAssetCode)\" >\r\n                        {{ item.FixedAssetName }}\r\n                    </td>\r\n                    <td @click=\"selectItem(index, item.FixedAssetCode)\" >\r\n                        {{ item.FixedAssetCategoryName }}\r\n                    </td>\r\n                    <td @click=\"selectItem(index, item.FixedAssetCode)\" >\r\n                        {{ item.DepartmentName }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\" >\r\n                        {{ item.Quantity }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\" >\r\n                        {{ this.formatMoney(item.Cost)\r\n\r\n                        }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\">\r\n                        {{ this.formatMoney(1200000) }}\r\n                    </td>\r\n                    <td class=\"text--right\" @click=\"selectItem(index, item.FixedAssetCode)\" >\r\n                        {{ this.formatMoney(item.Cost - 1200000) }}\r\n                    </td> -->\r\n                    <td>\r\n                        <div class=\" table__body--display\">\r\n                            <div class=\"icon body__table__icon--pencal\" @click=\"showPeltDetail(item)\" title=\"Chỉnh sửa\">\r\n                            </div>\r\n                            <div class=\"icon body__table__icon--duplicate\" title=\"Nhân bản\"></div>\r\n                        </div>\r\n                    </td>\r\n\r\n                </tr>\r\n\r\n\r\n\r\n                <div>\r\n\r\n                </div>\r\n\r\n            </tbody>\r\n\r\n            <div v-if=\"isNull\">\r\n                Rỗng\r\n            </div>\r\n\r\n\r\n            <tfoot class=\"table__footer \">\r\n                <tr>\r\n                    <td colspan=\"6\">\r\n                    </td>\r\n                    <td class=\"text--right table__footer__font--bold\">\r\n                        13\r\n                    </td>\r\n                    <td class=\"text--right table__footer__font--bold\">\r\n                        249.000.000\r\n                    </td>\r\n                    <td class=\"text--right table__footer__font--bold\">\r\n                        19.716.000\r\n                    </td>\r\n                    <td class=\"text--right table__footer__font--bold\">\r\n                        229.248.000\r\n                    </td>\r\n                    <td>\r\n\r\n                    </td>\r\n                </tr>\r\n            </tfoot>\r\n\r\n\r\n        </table>\r\n        <m-paging :totalRecordProp=\"this.totalRecord\" :keyWord=\"this.keyWordInput\" @sendData=\"bindDate\"\r\n            :isCallApi=\"this.isDeleteSucess\"></m-paging>\r\n    </div>\r\n\r\n    <div :style=\"[`top:${this.pageY}px; left:${this.pageX}px;`]\" class=\"contextmenu__container\" v-if=\"isShowMenuContext\">\r\n        <m-context-menu> </m-context-menu>\r\n    </div>\r\n</template>\r\n<script>\r\nimport pelfApi from '@/config/pelf/pelfapi';\r\nimport format from '@/js/format/format';\r\nimport resource from '@/js/resource/resource';\r\nimport MCheckBox from '@/components/base/MCheckBox.vue';\r\nimport MDropdown from '@/components/base/MDropdown.vue';\r\nimport MPaging from '@/components/base/MPaging.vue';\r\nimport MContextMenu from '@/components/base/MContextMenu.vue';\r\nexport default {\r\n    components: {\r\n        MCheckBox,\r\n        MDropdown,\r\n        MPaging,\r\n        MContextMenu\r\n    },\r\n\r\n    created() {\r\n        this.loadData();\r\n        this.$eventBus.on(\"contextMenuStatus\", (data) => {\r\n            this.contextMenuStatus = data;\r\n        });\r\n\r\n    },\r\n    watch: {\r\n        contextMenuStatus: function (newValue) {\r\n            this.actionContextMenu(newValue);\r\n        },\r\n        listChecked: function (newValue) {\r\n\r\n            this.$eventBus.emit(\"listCodeDelete\", newValue);\r\n        },\r\n        isDeleteSucess: function (newValue) {\r\n            if (newValue == true) {\r\n                this.listChecked.forEach(element => {\r\n                    this.listData.forEach(elementData => {\r\n                        if (element == elementData.FixedAssetCode) {\r\n                            this.listData.splice(this.data.indexOf(elementData), 1)\r\n                        }\r\n                    });\r\n                });\r\n\r\n            }\r\n            console.log(this.listData);\r\n            this.$emit(\"restartDeleteStatus\");\r\n        },\r\n        listData: function (newValue) {\r\n\r\n            if (newValue.length > 0) {\r\n                this.isNull = false\r\n            }\r\n            else {\r\n                this.isNull = true\r\n            }\r\n        }\r\n    },\r\n    props: {\r\n        isDeleteSucess: Boolean,\r\n\r\n    },\r\n\r\n    methods: {\r\n        /**\r\n         * Author:PXTRUONG(25/6/2023)\r\n         * Thực hiện chức năng của context theo trạng thái action\r\n         * @param {trạng thái muốn thực hiên:thêm,sửa.xóa} status \r\n         */\r\n        actionContextMenu(status) {\r\n            switch (status) {\r\n                // case this.$_MContextStatus.IsAdd:\r\n                //     console.log(\"thêm\");\r\n                //     break;\r\n                case this.$_MContextStatus.IsDelete:\r\n                    this.listChecked = [];\r\n                    this.listChecked.push(this.peltSellected.FixedAssetCode);\r\n                    this.$eventBus.emit(\"listCodeDelete\", this.listChecked);\r\n                    this.$eventBus.emit(\"contextMenuStatus\", null);\r\n                    break;\r\n                case this.$_MContextStatus.IsFixed:\r\n                 \r\n                    this.$emit(\"showDetail\", this.peltSellected, this.$_MFormMode.FixAsset);\r\n                    this.$eventBus.emit(\"contextMenuStatus\", null);\r\n                    break;\r\n                case this.$_MContextStatus.IsDuplicate:\r\n                    this.$emit(\"showDetail\", this.peltSellected, this.$_MFormMode.AddAsset);\r\n                    this.$eventBus.emit(\"contextMenuStatus\", null);\r\n                    break;\r\n\r\n\r\n\r\n            }\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(25/6/2023)\r\n         * Đóng mở contextMenu\r\n         */\r\n        showContextMenu(item) {\r\n            window.oncontextmenu = function (e) {\r\n                // Cancel the event\r\n                e.preventDefault();\r\n\r\n            };\r\n\r\n\r\n            if (!this.isShowMenuContext) {\r\n                console.log(item);\r\n                this.peltSellected = item;\r\n                this.pageX = event.pageX;\r\n                this.pageY = event.pageY + 15;\r\n                console.log(\"x\", event.pageX);\r\n                console.log(\"y\", event.pageY);\r\n                this.isShowMenuContext = true;\r\n            }\r\n            else {\r\n                this.isShowMenuContext = false;\r\n\r\n            }\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(25/6/2023)\r\n         * Nhấn ctrl để chỉ chọn 1 đối tượng\r\n         * @param {giá trị mã } data \r\n         * @param {vị trí của mã được chọn} index \r\n         */\r\n        oneSellected(data, index) {\r\n            this.firstSellect = index;\r\n\r\n            let item = []\r\n            item.push(data);\r\n            this.listChecked = item;\r\n\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(25/6/2023)\r\n         * Nhấn shift để chọn nhiều đối tượng \r\n         * @param {giá trị mã } data \r\n         * @param {vị trí của mã được chọn} index \r\n         */\r\n        moveSellected(data, index) {\r\n            var lastSelect = index + 1;\r\n            var list = [];\r\n            if (lastSelect < this.firstSellect) {\r\n                list = this.listData.slice(lastSelect - 1, this.firstSellect + 1);\r\n            }\r\n            else {\r\n                list = this.listData.slice(this.firstSellect, lastSelect);\r\n            }\r\n            let listDelete = []\r\n            list.forEach(element => {\r\n                listDelete.push(element.FixedAssetCode);\r\n            });\r\n            this.listChecked = listDelete;\r\n            console.log(this.listChecked);\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(24/6/2023)\r\n         * tìm các mã nhân viên có trong 1 trang \r\n         */\r\n        findFixedAssetCode() {\r\n            var data = [];\r\n            this.listData.forEach(element => {\r\n                data.push(element.FixedAssetCode);\r\n\r\n            });\r\n            console.log(data);\r\n            return data;\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(22/6/2023\r\n         * Thực hiện check tất cả \r\n         */\r\n        checkAll() {\r\n            if (this.ischeckall == true) {\r\n                // var listDelete = [];\r\n                // this.listData.forEach(element => {\r\n                //     listDelete.push(element.FixedAssetCode)\r\n\r\n                // });\r\n                // this.listChecked = listDelete\r\n                this.listChecked = this.listChecked.filter(item => !this.findFixedAssetCode().includes(item));\r\n\r\n                console.log(\"uncheck\", this.listChecked);\r\n            }\r\n            else {\r\n                this.listChecked = this.findFixedAssetCode();\r\n                console.log(this.listChecked);\r\n            }\r\n\r\n            // return listChecked;\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(21/6/2023)\r\n         * Chuyền dữ liệu sau khi phan trang \r\n         */\r\n        bindDate(data) {\r\n            this.listData = data;\r\n        },\r\n        /**\r\n        * Load dữ liệu từ api cho vào bảng\r\n        * Author:Phùng Xuân Trường\r\n        * 24/2/2023\r\n        */\r\n        async loadData() {\r\n            try {\r\n                await fetch(pelfApi.fixedAsset.getPaging + '?PageSize= 10' +\r\n                    '&PageNumber= 1')\r\n                    .then((res) => res.json())\r\n                    .then((value) => {\r