     ;\r\n      });\r\n      this.clostab();\r\n      this.isNull = false;\r\n\r\n    },\r\n  \r\n     /**\r\n      * Author:PXTRUONG(5/6/2023)\r\n      * nhận giá trị khi click vào item mk chọn \r\n      */\r\n    onSelectItem(item){\r\n        this.value=item[this.propText];\r\n        this.showClick();\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * validate dữ liệu \r\n     */\r\n    validate(data) {\r\n      if (this.isNotAllowNull) {\r\n        if (data == null || data == undefined || data == \"\") {\r\n          this.isNull = true;\r\n          this.alertMsg = this.labelContent + \" không được bỏ trống\"\r\n          return true;\r\n        }\r\n        else {\r\n          this.isNull = false;\r\n          return false;\r\n        }\r\n      }\r\n    }\r\n  },\r\n}\r\n</script>\r\n<style>\r\n@import url('../../style/base/combobox.css');\r\n@import url('../../style/base/icon.css');\r\n@import url('../../style/base/textfield.css');\r\n</style>"],"mappings":"AAgCA,SAASA,UAAS,QAAS,cAAc;AACzC,SAASC,SAAQ,QAAS,yBAAyB;AACnD,eAAe;EACbC,UAAU,EAAE;IACVF,UAAU;IACVC;EACF,CAAC;EACDE,KAAK,EAAE;IACLC,QAAQ,EAAE,IAAI;IACdC,YAAY,EAAE,IAAI;IAClBC,YAAY,EAAE,IAAI;IAClBC,SAAS,EAAEC,OAAO;IAClBC,cAAc,EAAED,OAAO;IACvBE,QAAQ,EAAC;EAEX,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACT,QAAQ,CAAC;IAC1B,IAAI,CAACU,UAAU,GAAC,IAAI,CAACV,QAAQ;IAC7B,IAAI,CAACW,YAAY,GAAC,IAAI,CAACX,QAAQ;EACjC,CAAC;EACDY,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,KAAK;MACbC,QAAQ,EAAE,CAAC;MACXC,KAAK,EAAE,IAAI;MACXC,MAAM,EAAE,KAAK;MACbC,QAAQ,EAAE,IAAI;MACdN,YAAY,EAAC,EAAE;MACfD,UAAU,EAAC;IAEb;EACF,CAAC;EACDQ,OAAO,EAAE;IACR;;;;IAICC,SAASA,CAAA,EAAG;MAEV,IAAI,IAAI,CAACN,MAAK,IAAK,KAAK,EAAE;QACxB,IAAI,CAACA,MAAK,GAAI,IAAI;MACpB,OACK;QACH,IAAI,CAACA,MAAK,GAAI,KAAK;MACrB;IAGF,CAAC;IACD;;;;IAIAO,YAAYA,CAACR,IAAI,EAAC;MACZ,IAAI,CAACD,YAAY,GAAC,IAAI,CAACD,UAAU,CAACW,MAAM,CAACC,IAAG,IAAKA,IAAI,CAAC,IAAI,CAAChB,QAAQ,CAAC,CAACiB,QAAQ,CAACX,IAAI,CAAE,CAAC;MACrF,IAAI,CAACC,MAAM,GAAC,IAAI;IACtB