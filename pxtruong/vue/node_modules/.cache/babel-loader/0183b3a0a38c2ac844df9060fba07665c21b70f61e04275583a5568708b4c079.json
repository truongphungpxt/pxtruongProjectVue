{"ast":null,"code":"import { ElDropdown } from \"element-plus\";\nimport { ArrowDown } from \"@element-plus/icons-vue\";\nexport default {\n  components: {\n    ElDropdown,\n    ArrowDown\n  },\n  props: {\n    listData: null,\n    labelContent: null,\n    iconDropdown: null,\n    isRequire: Boolean,\n    isNotAllowNull: Boolean,\n    propText: null\n  },\n  created() {\n    console.log(this.listData);\n    this.origilData = this.listData;\n    this.itermsFilter = this.listData;\n  },\n  data() {\n    return {\n      isshow: false,\n      nowindex: 0,\n      value: null,\n      isNull: false,\n      alertMsg: null,\n      itermsFilter: [],\n      origilData: []\n    };\n  },\n  methods: {\n    /**\r\n     * Author: PXTRUONG(5/6/2023)\r\n     * đóng mở danh list dropdown\r\n     */\n    showClick() {\n      if (this.isshow == false) {\n        this.isshow = true;\n      } else {\n        this.isshow = false;\n      }\n    },\n    /**\r\n     * Author:PXTURONG(5/6/2023)\r\n     * Autocomplite \r\n     */\n    filterIterms(data) {\n      this.itermsFilter = this.origilData.filter(item => item[this.propText].includes(data));\n      this.isshow = true;\n    },\n    clostab() {},\n    kewdown() {\n      if (this.nowindex < this.listData.length) {\n        this.nowindex++;\n      } else {\n        this.nowindex = 0;\n      }\n    },\n    enter() {\n      const dropdown1 = this.$refs[\"dropdown\"];\n      dropdown1.forEach(element => {\n        if (element.$attrs.class == \"color\") {\n          this.handleCommand(element.command);\n        }\n        ;\n      });\n      this.clostab();\n      this.isNull = false;\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * nhận giá trị khi click vào item mk chọn \r\n     */\n    onSelectItem(item) {\n      this.value = item[this.propText];\n      this.showClick();\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * validate dữ liệu \r\n     */\n    validate(data) {\n      if (this.isNotAllowNull) {\n        if (data == null || data == undefined || data == \"\") {\n          this.isNull = true;\n          this.alertMsg = this.labelContent + \" không được bỏ trống\";\n          return true;\n        } else {\n          this.isNull = false;\n          return false;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["ElDropdown","ArrowDown","components","props","listData","labelContent","iconDropdown","isRequire","Boolean","isNotAllowNull","propText","created","console","log","origilData","itermsFilter","data","isshow","nowindex","value","isNull","alertMsg","methods","showClick","filterIterms","filter","item","includes","clostab","kewdown","length","enter","dropdown1","$refs","forEach","element","$attrs","class","handleCommand","command","onSelectItem","validate","undefined"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\base\\MDropdown.vue"],"sourcesContent":["<template>\r\n  <div class=\"input__container \">\r\n    <div class=\"icon dropdown__icon--position \" :class=\"iconDropdown\" v-if=\"iconDropdown\"></div>\r\n    <p class=\"popup__font__label--name \" v-if=\"labelContent\">\r\n      {{ labelContent }}<span v-if=\"isNotAllowNull\" class=\"popup__color--red\">*</span>\r\n    </p>\r\n    <div class=\"mcombobox__container\">\r\n      <input class=\"input__field\" type=\"text\" :placeholder=\"placeholder\" :class=\"{ 'input__boder--red': isNull }\"\r\n      v-model=\"this.value\" @blur=\"validate(this.value)\" @keydown.down=\"kewdown\" @keydown.tab=\"clostab\"\r\n      @keyup.enter=\"enter\" @input=\"filterIterms(this.value)\"  />\r\n      <div @click=\"showClick\" class=\"mcombobox__icon--arrowdown  icon\" @keydown.down=\"kewdown\"></div>\r\n\r\n    </div>\r\n   \r\n    <div class=\"mcombobox__listd\" v-show=\"isshow\">\r\n      <div class=\"mcombobox__item\" v-for=\"(item) in itermsFilter\" :key=\"item\" @click=\"onSelectItem(item)\"  :class=\"{'is--focus': item[propText]== this.value}\" >\r\