{"ast":null,"code":"import format from '@/js/format/format';\nexport default {\n  props: {\n    modelValue: null,\n    inputContent: null,\n    isNotAllowNull: Boolean,\n    isInputNumber: {\n      type: Boolean,\n      default: false\n    },\n    isDisableAndPaddingRight: {\n      type: Boolean,\n      default: false\n    },\n    isPaddingRight: {\n      type: Boolean,\n      default: false\n    },\n    isNomalInput: {\n      type: Boolean,\n      default: false\n    },\n    isCheckValidate: {\n      type: Boolean,\n      default: false\n    },\n    setMaxLength: null,\n    maxQuantyti: {\n      type: Number\n    }\n  },\n  created() {\n    this.value = format.fomatmoney(this.modelValue);\n    this.displayNumber = this.modelValue;\n  },\n  watch: {\n    value: function (newValue) {\n      // this.validate(this.value);\n      var data;\n      data = format.unformat(newValue);\n      if (!this.checkNull(newValue)) {\n        this.validate(newValue);\n      }\n      data = Number(data);\n      console.log(data);\n      this.$emit(\"update:modelValue\", data);\n      this.value = format.fomatmoney(data);\n    },\n    modelValue: function (newValue) {\n      this.value = format.fomatmoney(newValue);\n    },\n    isCheckValidate: function (newValue) {\n      if (newValue && this.isNotAllowNull) {\n        this.validate(this.value);\n      }\n    }\n  },\n  methods: {\n    /**\r\n    * Author:PXTRUONG(27/6/2023)\r\n    * Thực hiện focus \r\n    */\n    focus() {\n      this.$nextTick(function () {\n        this.$refs[\"Minput\"].focus();\n      });\n    },\n    /**\r\n    * AuthorPXTRUONG(15/6/2023)\r\n    * Check null\r\n    */\n    checkNull(data) {\n      if (data == null || data == undefined || data == \"\") {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    /**\r\n     * Author:PXTRUONG, Create: 29/5/2023\r\n     * validate dữ liệu \r\n     */\n    validate(data) {\n      try {\n        data = format.unformat(data);\n        if (this.isNotAllowNull == true) {\n          if (data > this.maxQuantyti) {\n            // data=this.maxQuantyti;\n            this.isNull = true;\n            this.alertMsg = this.$_MResource.AlertContent.NotUpto + this.maxQuantyti;\n            return;\n          }\n          if (data == null || data == undefined || data == \"\") {\n            this.isNull = true;\n            this.alertMsg = this.inputContent + this.$_MResource.AlertContent.NotNull;\n            // this.$nextTick(function () {\n            //     this.$refs[\"Minput\"].focus();\n            // })\n            // console.log( this.$refs.Minput);\n            return true;\n          } else {\n            this.isNull = false;\n            return false;\n          }\n          // }\n        }\n      } catch (error) {}\n    },\n    /**\r\n     * Author:PXTRUONG(30/5/2023)\r\n     * Kiểm tra xem có value có phải dạng sô không\r\n     */\n    checkNumber(data) {\n      return Number.isFinite(data);\n    },\n    /**\r\n     * Author:PXTRUONG(30/5/2023)\r\n     * thực hiện tăng thêm 1 đơn vị khi input dạng số \r\n     */\n    btnUp() {\n      try {\n        if (this.isInputNumber) {\n          //lần đầu tiên chưa nhập số gì khi nhẫn nút tăng \n          if (this.validate(this.value)) {\n            this.value = 0;\n          }\n          this.value = format.unformat(this.value);\n          this.value += 1;\n          this.value = format.fomatmoney(this.value);\n          this.validate(this.value);\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    /**\r\n    * Author:PXTRUONG(30/5/2023)\r\n    * thực hiện giảm  thêm 1 đơn vị khi input dạng số \r\n    */\n    btnDown() {\n      try {\n        if (this.isInputNumber) {\n          if (this.value == null || this.value == undefined || this.value == \"\") {\n            this.value = 0;\n          }\n          // this.value = format.unformat(this.value)\n          // this.value = Number(this.value);\n          if (this.value > 0) {\n            this.value = format.unformat(this.value);\n            this.value -= 1;\n            this.value = format.fomatmoney(this.value);\n            this.validate(this.value);\n          } else {\n            return;\n          }\n        }\n      } catch (error) {}\n    }\n  },\n  data() {\n    return {\n      value: null,\n      isRequid: Boolean,\n      isNull: false,\n      alertMsg: null,\n      displayNumber: null\n    };\n  }\n};","map":{"version":3,"names":["format","props","modelValue","inputContent","isNotAllowNull","Boolean","isInputNumber","type","default","isDisableAndPaddingRight","isPaddingRight","isNomalInput","isCheckValidate","setMaxLength","maxQuantyti","Number","created","value","fomatmoney","displayNumber","watch","newValue","data","unformat","checkNull","validate","console","log","$emit","methods","focus","$nextTick","$refs","undefined","isNull","alertMsg","$_MResource","AlertContent","NotUpto","NotNull","error","checkNumber","isFinite","btnUp","btnDown","isRequid"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\base\\MInputNumber.vue"],"sourcesContent":["<template>\r\n    <div class=\"input__container\">\r\n        <p class=\"popup__font__label--name\">\r\n            {{ inputContent }} <span v-if=\"isNotAllowNull\" class=\"popup__color--red\">*</span>\r\n        </p>\r\n        <div>\r\n            <input v-if=\"isDisableAndPaddingRight == true\" disabled class=\"input__field bgr__color--gray set_text--left \"\r\n                v-model=\"this.value\" @blur=\"validate(this.value)\" :class=\"{ 'input__boder--red': isNull }\" ref=\"Minput\">\r\n\r\n            <input v-if=\"isPaddingRight == true\" class=\"input__field set_text--left \" v-model=\"this.value\"\r\n                @blur=\"validate(this.value)\" :class=\"{ 'input__boder--red': isNull }\" @keydown.up=\"btnUp\"\r\n                @keydown.down=\"btnDown\" ref=\"Minput\" :maxlength=\"setMaxLength\">\r\n\r\n            <div class=\"input__range\" v-if=\"isInputNumber\">\r\n                <div class=\" icon input__icon--dropdownup\" @click=\"btnUp\">\r\n\r\n                </div>\r\n                <div class=\"icon input__icon--dropdown\" @click=\"btnDown\">\r\n\r\n                </div>\r\n            </div>\r\n            <div class=\"popup__error\">\r\n                <div class=\"popup__color--red\" v-if=\"isNull\">\r\n                    {{ alertMsg }}\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n    </div>\r\n</template>\r\n<script>\r\nimport format from '@/js/format/format';\r\nexport default {\r\n    props: {\r\n        modelValue: null,\r\n        inputContent: null,\r\n        isNotAllowNull: Boolean,\r\n        isInputNumber: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        isDisableAndPaddingRight: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        isPaddingRight: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        isNomalInput: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        isCheckValidate: {\r\n            type: Boolean,\r\n            default: false\r\n        },\r\n        setMaxLength: null,\r\n        maxQuantyti: {\r\n            type: Number,\r\n        }\r\n    },\r\n    created() {\r\n        this.value = format.fomatmoney(this.modelValue);\r\n        this.displayNumber = this.modelValue;\r\n\r\n\r\n    },\r\n    watch: {\r\n        value: function (newValue) {\r\n            // this.validate(this.value);\r\n            var data;\r\n            data = format.unformat(newValue);\r\n\r\n            if (!this.checkNull(newValue)) {\r\n                this.validate(newValue);\r\n            }\r\n            data=Number(data);\r\n            console.log(data);\r\n            this.$emit(\"update:modelValue\", data);\r\n\r\n            this.value = format.fomatmoney(data);\r\n            \r\n        },\r\n        modelValue: function (newValue) {\r\n        \r\n           \r\n            this.value = format.fomatmoney(newValue);\r\n        },\r\n        isCheckValidate: function (newValue) {\r\n            if (newValue && this.isNotAllowNull) {\r\n                this.validate(this.value)\r\n\r\n            }\r\n        }\r\n\r\n    },\r\n    methods: {\r\n        /**\r\n        * Author:PXTRUONG(27/6/2023)\r\n        * Thực hiện focus \r\n        */\r\n        focus() {\r\n            this.$nextTick(function () {\r\n                this.$refs[\"Minput\"].focus();\r\n            })\r\n        },\r\n        /**\r\n       * AuthorPXTRUONG(15/6/2023)\r\n       * Check null\r\n       */\r\n        checkNull(data) {\r\n            if (data == null || data == undefined || data == \"\") {\r\n                return true;\r\n            }\r\n            else {\r\n                return false;\r\n            }\r\n        },\r\n        /**\r\n         * Author:PXTRUONG, Create: 29/5/2023\r\n         * validate dữ liệu \r\n         */\r\n        validate(data) {\r\n            try {\r\n                data = format.unformat(data);\r\n                if (this.isNotAllowNull == true) {\r\n                    if (data > this.maxQuantyti) {\r\n                        // data=this.maxQuantyti;\r\n                        this.isNull = true;\r\n                        this.alertMsg = this.$_MResource.AlertContent.NotUpto + this.maxQuantyti;\r\n                        return;\r\n                    }\r\n                    if (data == null || data == undefined || data == \"\") {\r\n                        this.isNull = true;\r\n                        this.alertMsg = this.inputContent + this.$_MResource.AlertContent.NotNull;\r\n                        // this.$nextTick(function () {\r\n                        //     this.$refs[\"Minput\"].focus();\r\n                        // })\r\n                        // console.log( this.$refs.Minput);\r\n                        return true;\r\n                    }\r\n                    else {\r\n                        this.isNull = false;\r\n\r\n                        return false;\r\n                    }\r\n                    // }\r\n\r\n                }\r\n            } catch (error) {\r\n\r\n            }\r\n\r\n\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(30/5/2023)\r\n         * Kiểm tra xem có value có phải dạng sô không\r\n         */\r\n        checkNumber(data) {\r\n            return Number.isFinite(data);\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(30/5/2023)\r\n         * thực hiện tăng thêm 1 đơn vị khi input dạng số \r\n         */\r\n        btnUp() {\r\n            try {\r\n                if (this.isInputNumber) {\r\n                    //lần đầu tiên chưa nhập số gì khi nhẫn nút tăng \r\n                    if (this.validate(this.value)) {\r\n                        this.value = 0;\r\n                    }\r\n                    this.value = format.unformat(this.value);\r\n\r\n                    this.value += 1;\r\n                   \r\n                    this.value = format.fomatmoney(this.value);\r\n                    this.validate(this.value);\r\n\r\n\r\n                }\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n\r\n\r\n\r\n        },\r\n\r\n        /**\r\n        * Author:PXTRUONG(30/5/2023)\r\n        * thực hiện giảm  thêm 1 đơn vị khi input dạng số \r\n        */\r\n        btnDown() {\r\n            try {\r\n                if (this.isInputNumber) {\r\n                    if (this.value == null || this.value == undefined || this.value == \"\") {\r\n                        this.value = 0;\r\n                    }\r\n                    // this.value = format.unformat(this.value)\r\n                    // this.value = Number(this.value);\r\n                    if (this.value > 0) {\r\n                        this.value = format.unformat(this.value);\r\n\r\n                        this.value -= 1;\r\n                       \r\n                        this.value = format.fomatmoney(this.value);\r\n                        this.validate(this.value);\r\n\r\n                    }\r\n\r\n                    else {\r\n\r\n                        return\r\n                    }\r\n\r\n                }\r\n            } catch (error) {\r\n\r\n            }\r\n\r\n\r\n        },\r\n\r\n\r\n    },\r\n\r\n    data() {\r\n        return {\r\n            value: null,\r\n            isRequid: Boolean,\r\n            isNull: false,\r\n            alertMsg: null,\r\n            displayNumber: null\r\n\r\n        }\r\n    },\r\n}\r\n</script>\r\n<style scoped>\r\n@import url(../../style/base/textfield.css);\r\n@import url(../../style/base/icon.css);\r\n</style>"],"mappings":"AA+BA,OAAOA,MAAK,MAAO,oBAAoB;AACvC,eAAe;EACXC,KAAK,EAAE;IACHC,UAAU,EAAE,IAAI;IAChBC,YAAY,EAAE,IAAI;IAClBC,cAAc,EAAEC,OAAO;IACvBC,aAAa,EAAE;MACXC,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDC,wBAAwB,EAAE;MACtBF,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDE,cAAc,EAAE;MACZH,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDG,YAAY,EAAE;MACVJ,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDI,eAAe,EAAE;MACbL,IAAI,EAAEF,OAAO;MACbG,OAAO,EAAE;IACb,CAAC;IACDK,YAAY,EAAE,IAAI;IAClBC,WAAW,EAAE;MACTP,IAAI,EAAEQ;IACV;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,KAAI,GAAIjB,MAAM,CAACkB,UAAU,CAAC,IAAI,CAAChB,UAAU,CAAC;IAC/C,IAAI,CAACiB,aAAY,GAAI,IAAI,CAACjB,UAAU;EAGxC,CAAC;EACDkB,KAAK,EAAE;IACHH,KAAK,EAAE,SAAAA,CAAUI,QAAQ,EAAE;MACvB;MACA,IAAIC,IAAI;MACRA,IAAG,GAAItB,MAAM,CAACuB,QAAQ,CAACF,QAAQ,CAAC;MAEhC,IAAI,CAAC,IAAI,CAACG,SAAS,CAACH,QAAQ,CAAC,EAAE;QAC3B,IAAI,CAACI,QAAQ,CAACJ,QAAQ,CAAC;MAC3B;MACAC,IAAI,GAACP,MAAM,CAACO,IAAI,CAAC;MACjBI,OAAO,CAACC,GAAG,CAACL,IAAI,CAAC;MACjB,IAAI,CAACM,KAAK,CAAC,mBAAmB,EAAEN,IAAI,CAAC;MAErC,IAAI,CAACL,KAAI,GAAIjB,MAAM,CAACkB,UAAU,CAACI,IAAI,CAAC;IAExC,CAAC;IACDpB,UAAU,EAAE,SAAAA,CAAUmB,QAAQ,EAAE;MAG5B,IAAI,CAACJ,KAAI,GAAIjB,MAAM,CAACkB,UAAU,CAACG,QAAQ,CAAC;IAC5C,