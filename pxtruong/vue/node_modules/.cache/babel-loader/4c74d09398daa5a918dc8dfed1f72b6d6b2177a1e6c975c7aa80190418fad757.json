is.isNull = true;\n            this.alertMsg = \" Nhập đúng định dạng số\";\n            return;\n          }\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    /**\r\n    * Author:PXTRUONG(30/5/2023)\r\n    * thực hiện giảm  thêm 1 đơn vị khi input dạng số \r\n    */\n    btnDown() {\n      try {\n        if (this.isInputNumber) {\n          if (this.value == null || this.value == undefined || this.value == \"\") {\n            this.value = 0;\n          }\n          this.value = Number(this.value);\n          if (this.checkNumber(this.value) == true && this.value > 0) {\n            this.validate(this.value);\n            this.value -= 1;\n          } else if (this.value <= 0) {\n            return;\n          } else {\n            this.value = \"\";\n            this.isNull = true;\n            this.alertMsg = \" Nhập đúng định dạng số\";\n            return;\n          }\n        }\n      } catch (error) {}\n    }\n  },\n  computed: {\n    displayNumber: function () {\n      if (this.value) {\n        let numberInput = format.fomatmoney(this.value);\n        console.log(numberInput);\n        return numberInput;\n      } else {\n        return;\n      }\n    }\n  },\n  data() {\n    return {\n      value: null,\n      isRequid: Boolean,\n      isNull: false,\n      alertMsg: null,\n      displayNumber: null\n    };\n  }\n};","map":{"version":3,"names":["format","props","modelValue","inputContent","isNotAllowNull","Boolean","isInputNumber","type","default","isDisableAndPaddingRight","isPaddingRight","isNomalInput","created","value","watch","newValue","$emit","methods","validate","data","undefined","isNull","alertMsg","error","checkNumber","Number","isFinite","btnUp","console","log","btnDown","computed","displayNumber","numberInput","fomatmoney","isRequid"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\base\\MInputNumber.vue"],"sourcesContent":["<template>\r\n    <