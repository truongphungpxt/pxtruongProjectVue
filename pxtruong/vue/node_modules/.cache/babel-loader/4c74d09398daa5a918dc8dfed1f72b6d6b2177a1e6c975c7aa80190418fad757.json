odelValue;\r\n        \r\n       \r\n    },\r\n    watch: {\r\n        value: function (newValue) {\r\n            this.$emit(\"update:modelValue\", newValue);\r\n        }\r\n    },\r\n    methods: {\r\n        /**\r\n         * Author:PXTRUONG, Create: 29/5/2023\r\n         * validate dữ liệu \r\n         */\r\n        validate(data) {\r\n            try {\r\n                if (this.isNotAllowNull == true) {\r\n                if (data == null || data == undefined || data == \"\") {\r\n                    this.isNull = true;\r\n                    this.alertMsg = this.inputContent + \" không được bỏ trống\"\r\n                    return true;\r\n                }\r\n                else {\r\n                    this.isNull = false;\r\n\r\n                    return false;\r\n                }\r\n                // }\r\n\r\n            }\r\n            } catch (error) {\r\n                \r\n            }\r\n           \r\n\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(30/5/2023)\r\n         * Kiểm tra xem có value có phải dạng sô không\r\n         */\r\n        checkNumber(data) {\r\n            return Number.isFinite(data);\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(30/5/2023)\r\n         * thực hiện tăng thêm 1 đơn vị khi input dạng số \r\n         */\r\n        btnUp() {\r\n            try {\r\n                if (this.isInputNumber) {\r\n                //lần đầu tiên chưa nhập số gì khi nhẫn nút tăng \r\n                if (this.validate(this.value)) {\r\n                    this.value = 0;\r\n                }\r\n                this.value = Number(this.value);\r\n                //kiểm tra số và tăng lên 1 đơn vị\r\n                if (this.checkNumber(this.value) == true) {\r\n\r\n                    this.value += 1;\r\n                    this.validate(this.value);\r\n\r\n                }\r\n                //sai định dạng thì thông báo \r\n                else {\r\n                    this.isNull = true\r\n                    this.alertMsg = \" Nhập đúng định dạng số\";\r\n\r\n                    return\r\n\r\n\r\n                }\r\n\r\n            }\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n           \r\n\r\n\r\n        },\r\n       \r\n        /**\r\n        * Author:PXTRUONG(30/5/2023)\r\n        * thực hiện giảm  thêm 1 đơn vị khi input dạng số \r\n        */\r\n        btnDown() {\r\n            try {\r\n                if (this.isInputNumber) {\r\n                if (this.value == null || this.value == undefined || this.value == \"\") {\r\n                    this.value = 0;\r\n                }\r\n                this.value = Number(this.value);\r\n                if (this.checkNumber(this.value) == true && this.value > 0) {\r\n\r\n                    this.validate(this.value);\r\n                    this.value -= 1;\r\n                }\r\n                else if (this.value <= 0) {\r\n\r\n                    return\r\n                }\r\n                else {\r\n                    this.value = \"\";\r\n                    this.isNull = true\r\n                    this.alertMsg = \" Nhập đúng định dạng số\";\r\n                    return\r\n                }\r\n            }\r\n            } catch (error) {\r\n                \r\n            }\r\n           \r\n\r\n        },\r\n\r\n\r\n    },\r\n    computed:{\r\n        displayNumber:function(){\r\n            if(this.value){\r\n                let numberInput= format.fomatmoney(this.value)\r\n                console.log(numberInput);\r\n                return numberInput;\r\n            }\r\n            else{\r\n                return ;\r\n            }\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            value: null,\r\n            isRequid: Boolean,\r\n            isNull: false,\r\n            alertMsg: null,\r\n            displayNumber:null\r\n\r\n        }\r\n    },\r\n}\r\n</script>\r\n<style scoped>\r\n@import url(../../style/base/textf