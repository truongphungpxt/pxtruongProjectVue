{"ast":null,"code":"import { ElDropdown } from \"element-plus\";\nimport { ArrowDown } from \"@element-plus/icons-vue\";\nexport default {\n  components: {\n    ElDropdown,\n    ArrowDown\n  },\n  props: {\n    labelContent: null,\n    iconDropdown: null,\n    isRequire: Boolean,\n    isNotAllowNull: Boolean,\n    propText: null,\n    api: null\n  },\n  created() {\n    console.log(this.api);\n    fetch(this.api).then(res => res.json()).then(data => {\n      this.listData = data;\n      this.origilData = data;\n      this.itermsFilter = data;\n    });\n  },\n  data() {\n    return {\n      isshow: false,\n      nowindex: 0,\n      value: \"\",\n      isNull: false,\n      alertMsg: null,\n      itermsFilter: [],\n      origilData: [],\n      listData: [],\n      indexSelected: 0\n    };\n  },\n  methods: {\n    /**\r\n     * Author: PXTRUONG(5/6/2023)\r\n     * đóng mở danh list dropdown\r\n     */\n    showClick() {\n      if (this.isshow == false) {\n        this.isshow = true;\n      } else {\n        this.isshow = false;\n      }\n    },\n    /**\r\n     * Author:PXTURONG(5/6/2023)\r\n     * Autocomplite \r\n     */\n    filterIterms(event) {\n      var value = event.target.value;\n      this.itermsFilter = this.origilData.filter(item => item[this.propText].includes(value));\n      this.value = value;\n      this.isshow = true;\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * Đóng form\r\n     */\n    clostab() {\n      this.isshow = false;\n    },\n    /**\r\n     * Author:PXTRUONG(5/5/2023)\r\n     * thực hiện phím tắt để chọn item \r\n     */\n    kewdown(event) {\n      const maxLength = this.filterIterms.length;\n      if (maxLength == 0) {\n        return;\n      } else {\n        if (event.keyCode == 40) {\n          this.isshow = true;\n          if (this.indexSelected <= maxLength) {\n            this.indexSelected++;\n          } else {\n            this.indexSelected = 0;\n          }\n        } else if (event.keyCode == 38) {\n          if (this.indexSelected > 0) {\n            this.indexSelected--;\n          } else {\n            this.indexSelected = maxLength + 1;\n          }\n        } else if (event.keyCode == 13) {\n          this.value = this.itermsFilter[this.indexSelected][this.propText];\n          this.clostab();\n        }\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * nhận giá trị khi click vào item mk chọn \r\n     */\n    async onSelectItem(item) {\n      try {\n        console.log(item[this.propText]);\n        this.value = item[this.propText];\n        this.validate(this.value);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * validate dữ liệu \r\n     */\n    validate(data) {\n      if (this.isNotAllowNull) {\n        if (data == null || data == undefined || data == \"\") {\n          this.isNull = true;\n          this.alertMsg = this.labelContent + \" không được bỏ trống\";\n          this.clostab();\n          return true;\n        } else {\n          this.isNull = false;\n          this.clostab();\n          return false;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["ElDropdown","ArrowDown","components","props","labelContent","iconDropdown","isRequire","Boolean","isNotAllowNull","propText","api","created","console","log","fetch","then","res","json","data","listData","origilData","itermsFilter","isshow","nowindex","value","isNull","alertMsg","indexSelected","methods","showClick","filterIterms","event","target","filter","item","includes","clostab","kewdown","maxLength","length","keyCode","onSelectItem","validate","error","undefined"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\base\\MDropdown.vue"],"sourcesContent":["<template>\r\n  <div class=\"input__container \">\r\n    <div class=\"icon dropdown__icon--position \" :class=\"iconDropdown\" v-if=\"iconDropdown\"></div>\r\n    <p class=\"popup__font__label--name \" v-if=\"labelContent\">\r\n      {{ labelContent }}<span v-if=\"isNotAllowNull\" class=\"popup__color--red\">*</span>\r\n    </p>\r\n    <div class=\"mcombobox__container\">\r\n      <input class=\"input__field\" type=\"text\" :class=\"{ 'input__boder--red': isNull }\" :value=\"this.value\"\r\n        @blur=\"validate(this.value)\" @input=\"filterIterms(this.value)\" @keydown=\"kewdown\" />\r\n      <div @click=\"showClick\" class=\"mcombobox__icon--arrowdown  icon\" @keydown.down=\"kewdown\"></div>\r\n\r\n    </div>\r\n\r\n    <div class=\"mcombobox__listd\" v-show=\"isshow\">\r\n      <div class=\"mcombobox__item\" v-for=\"(item,index) in itermsFilter\" :key=\"index\" @click=\"onSelectItem(item)\"\r\n        :class=\"{ 'is--focus': index== indexSelected }\">\r\n        {{\r\n          item[propText]\r\n        }}\r\n      </div>\r\n\r\n\r\n\r\n    </div>\r\n\r\n    <!-- <div class=\"popup__color--red\" v-if=\"isNull\">\r\n      {{ alertMsg }}\r\n    </div> -->\r\n\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport { ElDropdown } from \"element-plus\";\r\nimport { ArrowDown } from \"@element-plus/icons-vue\";\r\nexport default {\r\n  components: {\r\n    ElDropdown,\r\n    ArrowDown,\r\n  },\r\n  props: {\r\n    labelContent: null,\r\n    iconDropdown: null,\r\n    isRequire: Boolean,\r\n    isNotAllowNull: Boolean,\r\n    propText: null,\r\n\r\n    api: null\r\n\r\n  },\r\n  created() {\r\n    console.log(this.api);\r\n    fetch(this.api)\r\n      .then(res => res.json())\r\n      .then((data) => {\r\n        this.listData = data;\r\n        this.origilData = data;\r\n        this.itermsFilter = data;\r\n      })\r\n      ;\r\n\r\n\r\n\r\n  },\r\n  data() {\r\n    return {\r\n      isshow: false,\r\n      nowindex: 0,\r\n      value: \"\",\r\n      isNull: false,\r\n      alertMsg: null,\r\n      itermsFilter: [],\r\n      origilData: [],\r\n      listData: [],\r\n      indexSelected: 0\r\n\r\n    }\r\n  },\r\n  methods: {\r\n    /**\r\n     * Author: PXTRUONG(5/6/2023)\r\n     * đóng mở danh list dropdown\r\n     */\r\n    showClick() {\r\n\r\n      if (this.isshow == false) {\r\n\r\n        this.isshow = true;\r\n\r\n      }\r\n      else {\r\n        this.isshow = false;\r\n      }\r\n\r\n\r\n    },\r\n    /**\r\n     * Author:PXTURONG(5/6/2023)\r\n     * Autocomplite \r\n     */\r\n    filterIterms(event) {\r\n      var value= event.target.value\r\n      this.itermsFilter = this.origilData.filter(item => item[this.propText].includes(value));\r\n      this.value=value;\r\n      this.isshow = true;\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * Đóng form\r\n     */\r\n    clostab() {\r\n\r\n      this.isshow = false;\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(5/5/2023)\r\n     * thực hiện phím tắt để chọn item \r\n     */\r\n    kewdown(event) {\r\n      const maxLength = this.filterIterms.length;\r\n      if (maxLength == 0) {\r\n        return;\r\n      }\r\n      else {\r\n        if (event.keyCode == 40) {\r\n          this.isshow=true;\r\n          if(this.indexSelected<=maxLength)\r\n          {\r\n            this.indexSelected++;\r\n          }\r\n          else{\r\n            this.indexSelected=0;\r\n          }\r\n        }\r\n        else if(event.keyCode == 38){\r\n          if(this.indexSelected>0){\r\n            this.indexSelected--;\r\n          }\r\n          else{\r\n            this.indexSelected=maxLength +1;\r\n          }\r\n         \r\n        }\r\n        else if(event.keyCode == 13){\r\n          \r\n            this.value= this.itermsFilter[this.indexSelected][this.propText];\r\n            this.clostab();\r\n        }\r\n      }\r\n\r\n    },\r\n   \r\n\r\n\r\n\r\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * nhận giá trị khi click vào item mk chọn \r\n     */\r\n    async onSelectItem(item) {\r\n      try {\r\n        console.log(item[this.propText]);\r\n        this.value = item[this.propText];\r\n        this.validate(this.value);\r\n\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * validate dữ liệu \r\n     */\r\n    validate(data) {\r\n      if (this.isNotAllowNull) {\r\n        if (data == null || data == undefined || data == \"\") {\r\n          this.isNull = true;\r\n          this.alertMsg = this.labelContent + \" không được bỏ trống\"\r\n          this.clostab();\r\n          return true;\r\n        }\r\n        else {\r\n          this.isNull = false;\r\n          this.clostab();\r\n          return false;\r\n        }\r\n      }\r\n\r\n    }\r\n  },\r\n}\r\n</script>\r\n<style>\r\n@import url('../../style/base/combobox.css');\r\n@import url('../../style/base/icon.css');\r\n@import url('../../style/base/textfield.css');\r\n</style>"],"mappings":"AAiCA,SAASA,UAAS,QAAS,cAAc;AACzC,SAASC,SAAQ,QAAS,yBAAyB;AACnD,eAAe;EACbC,UAAU,EAAE;IACVF,UAAU;IACVC;EACF,CAAC;EACDE,KAAK,EAAE;IACLC,YAAY,EAAE,IAAI;IAClBC,YAAY,EAAE,IAAI;IAClBC,SAAS,EAAEC,OAAO;IAClBC,cAAc,EAAED,OAAO;IACvBE,QAAQ,EAAE,IAAI;IAEdC,GAAG,EAAE;EAEP,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACH,GAAG,CAAC;IACrBI,KAAK,CAAC,IAAI,CAACJ,GAAG,EACXK,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACtBF,IAAI,CAAEG,IAAI,IAAK;MACd,IAAI,CAACC,QAAO,GAAID,IAAI;MACpB,IAAI,CAACE,UAAS,GAAIF,IAAI;MACtB,IAAI,CAACG,YAAW,GAAIH,IAAI;IAC1B,CAAC;EAKL,CAAC;EACDA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLI,MAAM,EAAE,KAAK;MACbC,QAAQ,EAAE,CAAC;MACXC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,KAAK;MACbC,QAAQ,EAAE,IAAI;MACdL,YAAY,EAAE,EAAE;MAChBD,UAAU,EAAE,EAAE;MACdD,QAAQ,EAAE,EAAE;MACZQ,aAAa,EAAE;IAEjB;EACF,CAAC;EACDC,OAAO,EAAE;IACP;;;;IAIAC,SAASA,CAAA,EAAG;MAEV,IAAI,IAAI,CAACP,MAAK,IAAK,KAAK,EAAE;QAExB,IAAI,CAACA,MAAK,GAAI,IAAI;MAEpB,OACK;QACH,IAAI,CAACA,MAAK,GAAI,KAAK;MACrB;IAGF,CAAC;IACD;;;;IAIAQ,YAAYA,CAACC,KAAK,EAAE;MAClB,IAAIP,KAAK,GAAEO,KAAK,CAACC,MAAM,CAACR,KAAI;MAC5B,IAAI,CAACH,YAAW,GAAI,IAAI,CAACD,UAAU,CAACa,MAAM,CAACC,IAAG,IAAKA,IAAI,CAAC,IAAI,CAACzB,QAAQ,CAAC,CAAC0B,Q