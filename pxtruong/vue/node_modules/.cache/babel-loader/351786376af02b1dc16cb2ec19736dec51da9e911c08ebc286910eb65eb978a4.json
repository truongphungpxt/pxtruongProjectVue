;\r\n\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * validate dữ liệu \r\n     */\r\n    validate(data) {\r\n      if (this.isNotAllowNull) {\r\n        if (data == null || data == undefined || data == \"\") {\r\n          this.isNull = true;\r\n          this.alertMsg = this.labelContent + \" không được bỏ trống\"\r\n          this.clostab();\r\n          return true;\r\n        }\r\n        else {\r\n          this.isNull = false;\r\n          this.clostab();\r\n          return false;\r\n        }\r\n      }\r\n\r\n    }\r\n  },\r\n}\r\n</script>\r\n<style>\r\n@import url('../../style/base/combobox.css');\r\n@import url('../../style/base/icon.css');\r\n@import url('../../style/base/textfield.css');\r\n</style>"],"mappings":"AAuCA,SAASA,UAAS,QAAS,cAAc;AACzC,SAASC,SAAQ,QAAS,yBAAyB;AACnD,eAAe;EACbC,UAAU,EAAE;IACVF,UAAU;IACVC;EACF,CAAC;EACDE,KAAK,EAAE;IACLC,YAAY,EAAE,IAAI;IAClBC,YAAY,EAAE,IAAI;IAClBC,SAAS,EAAEC,OAAO;IAClBC,cAAc,EAAED,OAAO;IACvBE,QAAQ,EAAE,IAAI;IAEdC,GAAG,EAAE;EAEP,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACH,GAAG,CAAC;IACrBI,KAAK,CAAC,IAAI,CAACJ,GAAG,EACXK,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACtBF,IAAI,CAAEG,IAAI,IAAK;MACd,IAAI,CAACC,QAAO,GAAID,IAAI;MACpB,IAAI,CAACE,UAAS,GAAIF,IAAI;MACtB,IAAI,CAACG,YAAW,GAAIH,IAAI;IAC1B,CAAC;EAKL,CAAC;EACDA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLI,MAAM,EAAE,KAAK;MACbC,QAAQ,EAAE,CAAC;MACXC,KAAK,EAAC,IAAI;MACVC,MAAM,EAAE,KAAK;MACbC,QAAQ,EAAE,IAAI;MACdL,YAAY,EAAE,EAAE;MAChBD,UAAU,EAAE,EAAE;MACdD,QAAQ,EAAE,EAAE;MACZQ,aAAa,EAAE;IAEjB;EACF,CAAC;EACDC,OAAO,EAAE;IACP;;;;IAIAC,SAASA,CAAA,EAAG;MAEV,IAAI,IAAI,CAACP,MAAK,IAAK,KAAK,EAAE;QAExB,IAAI,CAACA,MAAK,GAAI,IAAI;MAEpB,OACK;QACH,IAAI,CAACA,MAAK,GAAI,KAAK;MACrB;IAGF,CAAC;IACD;;;;IAIAQ,YAAYA,CAACZ,IAAI,EAAE;MAEjB,IAAI,CAACG,YAAW,GAAI,IAAI,CAACD,UAAU,CAACW,MAAM,CAACC,IAAG,IAAKA,IAAI,CAAC,IAAI,CAACvB,QAAQ,CAAC,CAACwB,QAAQ,CAACf,IAAI,CAAC,CAAC;MAEtF,IAAI,CAACI,MAAK