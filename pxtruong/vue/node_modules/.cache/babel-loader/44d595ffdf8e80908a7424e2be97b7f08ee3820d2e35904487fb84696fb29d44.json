{"ast":null,"code":"import { ElDropdown } from \"element-plus\";\nimport { ArrowDown } from \"@element-plus/icons-vue\";\nexport default {\n  components: {\n    ElDropdown,\n    ArrowDown\n  },\n  props: {\n    labelContent: null,\n    iconDropdown: null,\n    isRequire: Boolean,\n    isNotAllowNull: Boolean,\n    propText: null,\n    api: null\n  },\n  created() {\n    this.loadata(this.api);\n    this.origilData = this.listData;\n    this.itermsFilter = this.listData;\n    console.log(\"danh sách \", this.itermsFilter);\n  },\n  data() {\n    return {\n      isshow: false,\n      nowindex: 0,\n      value: null,\n      isNull: false,\n      alertMsg: null,\n      itermsFilter: [],\n      origilData: [],\n      listData: []\n    };\n  },\n  methods: {\n    /**\r\n     * Author: PXTRUONG(5/6/2023)\r\n     * đóng mở danh list dropdown\r\n     */\n    showClick() {\n      if (this.isshow == false) {\n        this.isshow = true;\n      } else {\n        this.isshow = false;\n      }\n    },\n    /**\r\n     * Author:PXTURONG(5/6/2023)\r\n     * Autocomplite \r\n     */\n    filterIterms(data) {\n      this.itermsFilter = this.origilData.filter(item => item[this.propText].includes(data));\n      this.isshow = true;\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * Đóng form\r\n     */\n    clostab() {\n      this.isshow = false;\n    },\n    /**\r\n     * Author:PXTRUONG(5/5/2023)\r\n     * nhấn mũi tên xuống để mở form và focus vào item\r\n     */\n    kewdown() {\n      if (this.nowindex < this.listData.length) {\n        this.nowindex++;\n      } else {\n        this.nowindex = 0;\n      }\n    },\n    enter() {\n      const dropdown1 = this.$refs[\"dropdown\"];\n      dropdown1.forEach(element => {\n        if (element.$attrs.class == \"color\") {\n          this.handleCommand(element.command);\n        }\n        ;\n      });\n      this.clostab();\n      this.isNull = false;\n    },\n    /**\r\n     * Author:PXTRUONG(6/6/2023)\r\n     */\n    async loadata(api) {\n      await fetch(api).then(res => res.json()).then(data => {\n        this.listData = data;\n      });\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * nhận giá trị khi click vào item mk chọn \r\n     */\n    onSelectItem(item) {\n      this.value = item[this.propText];\n      this.validate(this.value);\n      this.clostab();\n    },\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * validate dữ liệu \r\n     */\n    validate(data) {\n      if (this.isNotAllowNull) {\n        if (data == null || data == undefined || data == \"\") {\n          this.isNull = true;\n          this.alertMsg = this.labelContent + \" không được bỏ trống\";\n          this.clostab();\n          return true;\n        } else {\n          this.isNull = false;\n          this.clostab();\n          return false;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["ElDropdown","ArrowDown","components","props","labelContent","iconDropdown","isRequire","Boolean","isNotAllowNull","propText","api","created","loadata","origilData","listData","itermsFilter","console","log","data","isshow","nowindex","value","isNull","alertMsg","methods","showClick","filterIterms","filter","item","includes","clostab","kewdown","length","enter","dropdown1","$refs","forEach","element","$attrs","class","handleCommand","command","fetch","then","res","json","onSelectItem","validate","undefined"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\base\\MDropdown.vue"],"sourcesContent":["<template>\r\n  <div class=\"input__container \">\r\n    <div class=\"icon dropdown__icon--position \" :class=\"iconDropdown\" v-if=\"iconDropdown\"></div>\r\n    <p class=\"popup__font__label--name \" v-if=\"labelContent\">\r\n      {{ labelContent }}<span v-if=\"isNotAllowNull\" class=\"popup__color--red\">*</span>\r\n    </p>\r\n    <div class=\"mcombobox__container\">\r\n      <input class=\"input__field\" type=\"text\" :class=\"{ 'input__boder--red': isNull }\"\r\n      v-bind=\"this.value\" @blur=\"validate(this.value)\" @keydown.down=\"kewdown\" @keydown.tab=\"clostab\"\r\n      @keyup.enter=\"enter\" @input=\"filterIterms(this.value)\"  />\r\n      <div @click=\"showClick\" class=\"mcombobox__icon--arrowdown  icon\" @keydown.down=\"kewdown\"></div>\r\n\r\n    </div>\r\n   \r\n    <div class=\"mcombobox__listd\" v-show=\"isshow\">\r\n      <div class=\"mcombobox__item\" v-for=\"(item) in itermsFilter\" :key=\"item\" @click=\"onSelectItem(item)\"  :class=\"{'is--focus': item[propText]== this.value}\" >\r\n        {{\r\n             item[propText]\r\n           }}\r\n      </div>\r\n      \r\n\r\n\r\n    </div>\r\n\r\n    <!-- <div class=\"popup__color--red\" v-if=\"isNull\">\r\n      {{ alertMsg }}\r\n    </div> -->\r\n   \r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport { ElDropdown } from \"element-plus\";\r\nimport { ArrowDown } from \"@element-plus/icons-vue\";\r\nexport default {\r\n  components: {\r\n    ElDropdown,\r\n    ArrowDown,\r\n  },\r\n  props: {\r\n    labelContent: null,\r\n    iconDropdown: null,\r\n    isRequire: Boolean,\r\n    isNotAllowNull: Boolean,\r\n    propText:null,\r\n\r\n    api:null\r\n\r\n  },\r\n  created() {\r\n   this.loadata(this.api);\r\n    this.origilData=this.listData;\r\n    this.itermsFilter=this.listData;\r\n    console.log( \"danh sách \",this.itermsFilter);\r\n  },\r\n  data() {\r\n    return {\r\n      isshow: false,\r\n      nowindex: 0,\r\n      value: null,\r\n      isNull: false,\r\n      alertMsg: null,\r\n      itermsFilter:[],\r\n      origilData:[],\r\n      listData:[]\r\n\r\n    }\r\n  },\r\n  methods: {\r\n   /**\r\n    * Author: PXTRUONG(5/6/2023)\r\n    * đóng mở danh list dropdown\r\n    */\r\n    showClick() {\r\n      \r\n      if (this.isshow == false) {\r\n        this.isshow = true;\r\n      }\r\n      else {\r\n        this.isshow = false;\r\n      }\r\n\r\n\r\n    },\r\n    /**\r\n     * Author:PXTURONG(5/6/2023)\r\n     * Autocomplite \r\n     */\r\n    filterIterms(data){\r\n          this.itermsFilter=this.origilData.filter(item => item[this.propText].includes(data) );\r\n          this.isshow=true;\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * Đóng form\r\n     */\r\n    clostab() {\r\n      \r\n      this.isshow=false;\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(5/5/2023)\r\n     * nhấn mũi tên xuống để mở form và focus vào item\r\n     */\r\n    kewdown() {\r\n     \r\n      if (this.nowindex < this.listData.length) {\r\n\r\n        this.nowindex++;\r\n\r\n      }\r\n\r\n      else {\r\n        this.nowindex = 0;\r\n      }\r\n    },\r\n    enter() {\r\n      const dropdown1 = this.$refs[\"dropdown\"];\r\n      dropdown1.forEach(element => {\r\n        if (element.$attrs.class == \"color\") {\r\n          this.handleCommand(element.command);\r\n\r\n        }\r\n        ;\r\n      });\r\n      this.clostab();\r\n      this.isNull = false;\r\n\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(6/6/2023)\r\n     */\r\n    async loadata(api ) {\r\n      await fetch(api).then(res => res.json())\r\n      .then((data)=>{\r\n        this.listData=data;\r\n      })\r\n      ;\r\n           \r\n        },\r\n\r\n  \r\n     /**\r\n      * Author:PXTRUONG(5/6/2023)\r\n      * nhận giá trị khi click vào item mk chọn \r\n      */\r\n    onSelectItem(item){\r\n        this.value=item[this.propText];\r\n        this.validate(this.value);\r\n        this.clostab();\r\n    },\r\n    /**\r\n     * Author:PXTRUONG(5/6/2023)\r\n     * validate dữ liệu \r\n     */\r\n    validate(data) {\r\n      if (this.isNotAllowNull) {\r\n        if (data == null || data == undefined || data == \"\") {\r\n          this.isNull = true;\r\n          this.alertMsg = this.labelContent + \" không được bỏ trống\"\r\n          this.clostab();\r\n          return true;\r\n        }\r\n        else {\r\n          this.isNull = false;\r\n          this.clostab();\r\n          return false;\r\n        }\