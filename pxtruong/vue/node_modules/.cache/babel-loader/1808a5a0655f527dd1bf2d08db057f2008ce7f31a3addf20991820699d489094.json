{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport format from '@/js/format/format';\nimport MAlert from '@/components/base/MAlert.vue';\nimport pelfApi from '@/config/pelf/pelfapi';\nimport Minput from \"@/components/base/Minput.vue\";\nimport MinputNumber from \"@/components/base/MInputNumber.vue\";\nimport MDropdown from \"@/components/base/MDropdown.vue\";\nimport MDatapicker from \"@/components/base/MDatapicker.vue\";\nimport MNotification from '@/components/base/MNotification.vue';\nimport resource from \"@/js/resource/resource\";\nimport maxlengthinput from \"@/js/enum/maxlengthinput.js\";\nexport default {\n  components: {\n    Minput,\n    MDropdown,\n    MDatapicker,\n    MinputNumber,\n    MAlert,\n    MNotification\n  },\n  props: {\n    pelf: null,\n    formMode: null\n  },\n  created() {\n    var now = new Date();\n    this.pelftDefault = JSON.stringify(this.pelf);\n    this.pelftDefault = JSON.parse(this.pelftDefault);\n    this.pelfInput = JSON.stringify(this.pelf);\n    this.pelfInput = JSON.parse(this.pelfInput);\n    if (this.formMode == this.$_MFormMode.FixAsset) {\n      this.popupTitle = resource.FormModeTitle.FixAsset;\n    } else if (this.formMode == this.$_MFormMode.AddAsset) {\n      this.popupTitle = resource.FormModeTitle.AddAsset;\n      this.pelfInput.TrackedYear = now.getFullYear();\n    }\n    console.log(this.pelf);\n    console.log(this.formMode);\n    this.atrophyYear = this.pelfInput.Cost * this.pelfInput.DepreciationRate / 100;\n  },\n  methods: {\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Đóng mở cảnh báo \r\n     */\n    closeAlert(isForm) {\n      if (isForm) {\n        this.isShowAlert = false;\n        return;\n      }\n      if (this.inValue) {\n        this.isShowAlert = false;\n      } else {\n        this.isShowAlert = false;\n        this.btnClose();\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Add list invalid vào 1 mảng \r\n     */\n    addListInValues(data, isPush) {\n      if (isPush) {\n        this.listInValue.push(data);\n      } else {\n        this.listInValue = this.listInValue.filter(item => item !== data);\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Check null\r\n     */\n    checkNull(data) {\n      data = format.whiteSpace(data);\n      if (!data) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    /**\r\n     * đóng form chi tiết tài sản \r\n     * Author: PXTRUONG, Create: 24/3/2023\r\n     */\n    btnClose() {\n      this.$emit(\"hiddePolfDetail\");\n    },\n    /**\r\n     * Author:PXTRUONG(15/6/2023)\r\n     * Thực hiện gọi api thêm tài sản \r\n     */\n    async add() {\n      let option = {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json;charset=utf-8'\n        },\n        body: JSON.stringify(this.pelfInput)\n      };\n      console.log(JSON.stringify(this.pelfInput));\n      await fetch(pelfApi.fixedAsset.api, option).then(res => res.json()).then(data => {\n        if (data.IsSuccess) {\n          this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Add);\n        } else {\n          this.showInform(true, this.$_MInformStatus.IsFailed, data.UserMessage);\n        }\n      });\n    },\n    /**\r\n    * Author:PXTRUONG(15/6/2023)\r\n    * Thực hiện gọi api sửa tài sản \r\n    */\n    async fix() {\n      let optionFix = {\n        method: 'PUT',\n        headers: {\n          'Content-Type': 'application/json;charset=utf-8'\n        },\n        body: JSON.stringify(this.pelfInput)\n      };\n      console.log(JSON.stringify(this.pelfInput));\n      await fetch(pelfApi.fixedAsset.api, optionFix).then(res => res.json()).then(data => {\n        if (data.IsSuccess) {\n          this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Fixed);\n        } else {\n          this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsFailed, this.$_MResource.InformContent.Fixed);\n        }\n      });\n    },\n    /**\r\n    * Author:PXTRUONG(19/6/2023)\r\n    * Thực hiện đóng mở thông báo chuyền nội dung thông báo \r\n    */\n    showInform(isShow, informStatus, content) {\n      this.isShowInform = isShow;\n      this.informStatus = informStatus;\n      this.informContent = content;\n      setTimeout(() => this.isShowInform = false, 4000);\n    },\n    /**\r\n     * Author:PXTRUONG(31/5/2023)\r\n     * Thực hiện lưu và đóng form \r\n     */\n    async btnSave() {\n      // this.btnClose();\n      try {\n        this.isCheckValidate = true;\n        for (const inputRef in this.$refs) {\n          //tìm các input invalid\n          if (this.checkNull(this.$refs[inputRef].value)) {\n            console.log(this.$refs[inputRef].inputContent);\n            this.inValue = true;\n            this.isShowAlert = true;\n            this.alertContent = this.$refs[inputRef].inputContent + \" không được bỏ trống\";\n            return;\n          }\n        }\n\n        // console.log(this.$refs[\"PurchaseDate\"]);\n        if (this.formMode == this.$_MFormMode.FixAsset) {\n          this.fix();\n        } else if (this.formMode == this.$_MFormMode.AddAsset) {\n          this.add();\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    /**\r\n     * Author:PXTRUONG(31/5/2023)\r\n     * Đống form và không lưu lại dữ liệu \r\n     */\n    btnCloseUnSave() {\n      if (this.formMode == this.$_MFormMode.FixAsset) {\n        this.statusAlert = this.$_MStatusAlert.IsFixAsset;\n        this.alertContent = null;\n        this.inValue = false;\n        this.isShowAlert = true;\n      } else if (this.formMode == this.$_MFormMode.AddAsset) {\n        this.statusAlert = this.$_MStatusAlert.IsAddFixAsset;\n        this.alertContent = null;\n        this.inValue = false;\n        this.isShowAlert = true;\n      }\n\n      // this.$emit(\"update:pelf\", this.pelftDefault);\n    },\n\n    /**\r\n     * \r\n     * Author:PXTRUONG(31/5/2023)\r\n     * thay đổi dữ liệu phụ thuộc khi thay đổi mã tài sản,loại tài sản \r\n     */\n    async bindData(api, value, protext) {\n      let option = {\n        method: 'GET',\n        headers: {\n          'Content-Type': 'application/json;charset=utf-8'\n        }\n      };\n      await fetch(api + \"?recodeCode=\" + value, option).then(res => res.json()).then(data => {\n        data.forEach(element => {\n          if (protext == \"DepartmentCode\") {\n            this.pelfInput.DepartmentName = element.DepartmentName;\n            this.pelfInput.DepartmentID = element.DepartmentID;\n          } else if (protext == \"FixedAssetCategoryCode\") {\n            this.pelfInput.FixedAssetCategoryName = element.FixedAssetCategoryName;\n            this.pelfInput.DepreciationRate = element.DepreciationRate;\n            this.pelfInput.LifeTime = element.LifeTime;\n            this.pelfInput.FixedAssetCategoryID = element.FixedAssetCategoryID;\n          }\n        });\n      });\n    }\n  },\n  data() {\n    return {\n      pelfInput: {},\n      pelftDefault: {},\n      listDepartment: [],\n      popupTitle: null,\n      atrophyYear: null,\n      department: [],\n      fixedAssetCategory: [],\n      data: {},\n      IsAllow: false,\n      isCheckValidate: false,\n      listInValue: [],\n      isShowAlert: false,\n      statusAlert: null,\n      alertContent: null,\n      inValue: false,\n      informStatus: null,\n      isShowInform: false,\n      informContent: null\n    };\n  }\n};","map":{"version":3,"names":["format","MAlert","pelfApi","Minput","MinputNumber","MDropdown","MDatapicker","MNotification","resource","maxlengthinput","components","props","pelf","formMode","created","now","Date","pelftDefault","JSON","stringify","parse","pelfInput","$_MFormMode","FixAsset","popupTitle","FormModeTitle","AddAsset","TrackedYear","getFullYear","console","log","atrophyYear","Cost","DepreciationRate","methods","closeAlert","isForm","isShowAlert","inValue","btnClose","addListInValues","data","isPush","listInValue","push","filter","item","checkNull","whiteSpace","$emit","add","option","method","headers","body","fetch","fixedAsset","api","then","res","json","IsSuccess","$_MInformStatus","IsSucess","$_MResource","InformContent","Add","showInform","IsFailed","UserMessage","fix","optionFix","Fixed","isShow","informStatus","content","isShowInform","informContent","setTimeout","btnSave","isCheckValidate","inputRef","$refs","value","inputContent","alertContent","error","btnCloseUnSave","statusAlert","$_MStatusAlert","IsFixAsset","IsAddFixAsset","bindData","protext","forEach","element","DepartmentName","DepartmentID","FixedAssetCategoryName","LifeTime","FixedAssetCategoryID","listDepartment","department","fixedAssetCategory","IsAllow"],"sources":["C:\\Users\\Admin\\Desktop\\Web-04-PXTRUONG\\web-04-vue\\web202304\\pxtruong\\vue\\src\\components\\view\\pelf\\PelfDetail.vue"],"sourcesContent":["<template>\r\n    <div class=\"body__popup\" id=\"detailEmployee\" tabindex=\"-1\" @keydown.ctrl.s=\"thu\">\r\n        <div class=\"popup\">\r\n            <div class=\"popup__body\">\r\n                <div class=\"popup__body__header\">\r\n                    <p class=\"popup__font--bold\">{{ popupTitle }}\r\n                    </p>\r\n                    <div class=\"icon popup__icon--cancal\" title=\"Đóng form\" @click=\"btnClose\"></div>\r\n                </div>\r\n\r\n                <div class=\"popup__body__container\">\r\n                    <div class=\"popup__body__container--left\">\r\n\r\n                        <minput input-content=\"Mã tài sản\" :is-input-number=\"false\" :is-not-allow-null=\"true\"\r\n                            :is-focus=\"true\" :is-nomal-input=\"true\" v-model=\"pelfInput.FixedAssetCode\"\r\n                            :is-check-validate=\"this.isCheckValidate\" ref=\"FixedAssetCode\" :setMaxLength=\"this.$_MMaxLengthInput.RecodeCode\" tabindex=\"1\">\r\n\r\n                        </minput>\r\n\r\n                        <m-dropdown input-content=\"Mã bộ phận sử dụng \" :is-not-allow-null=\"true\" prop-text=\"DepartmentCode\"\r\n                            :api=this.$_MApi.department.api v-model=\"pelfInput.DepartmentCode\" @sendData=\"bindData\"\r\n                            :is-check-validate=\"this.isCheckValidate\" ref=\"DepartmentCode\" :setMaxLength=\"this.$_MMaxLengthInput.RecodeCode\">\r\n\r\n                        </m-dropdown>\r\n                        <m-dropdown input-content=\" Mã loại tài sản \" :is-not-allow-null=\"true\"\r\n                            prop-text=\"FixedAssetCategoryCode\" :api=this.$_MApi.fixedAssetCategory.api\r\n                            v-model=\"pelfInput.FixedAssetCategoryCode\" @sendData=\"bindData\"\r\n                            :is-check-validate=\"this.isCheckValidate\" ref=\"FixedAssetCategoryCode\" :set-max-length=\"this.$_MMaxLengthInput.RecodeCode\">\r\n\r\n                        </m-dropdown>\r\n                        <minput-number input-content=\"Số lượng\" :is-input-number=\"true\" :is-not-allow-null=\"true\"\r\n                            :is-padding-right=\"true\" v-model=\"pelfInput.Quantity\" :is-check-validate=\"this.isCheckValidate\"\r\n                            ref=\"Quantity\" :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                        </minput-number>\r\n                        <minput-number input-content=\"Tỷ lệ hao mòn(%)\" :is-input-number=\"true\" :is-not-allow-null=\"true\"\r\n                            :is-padding-right=\"true\" v-model=\"pelfInput.DepreciationRate\"\r\n                            :is-check-validate=\"this.isCheckValidate\" ref=\"DepreciationRate\" :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                        </minput-number>\r\n                        <m-datapicker input-content=\"Ngày mua \" :is-not-allow-null=\"true\" v-model=\"pelfInput.PurchaseDate\"\r\n                            :is-check-validate=\"this.isCheckValidate\" ref=\"PurchaseDate\" :setMaxLength=\"this.$_MMaxLengthInput.number\"></m-datapicker>\r\n\r\n\r\n                    </div>\r\n                    <div class=\"popup__body__container--right\">\r\n                        <minput input-content=\"Tên tài sản \" :is-not-allow-null=\"true\" v-model=\"pelfInput.FixedAssetName\"\r\n                            :is-nomal-input=\"true\" :is-check-validate=\"this.isCheckValidate\" ref=\"FixedAssetName\" :setMaxLength=\"this.$_MMaxLengthInput.Nameother\" tabindex=\"2\">\r\n\r\n                        </minput>\r\n                        <minput input-content=\"Tên phòng ban \" :is-input-number=\"false\" :is-not-allow-null=\"true\"\r\n                            :is-disable=\"true\" v-model=\"pelfInput.DepartmentName\" :is-check-validate=\"this.isCheckValidate\"\r\n                            ref=\"DepartmentName\" >\r\n\r\n                        </minput>\r\n                        <minput input-content=\"Tên loại tài sản \" :is-input-number=\"false\" :is-not-allow-null=\"true\"\r\n                            :is-disable=\"true\" v-model=\"pelfInput.FixedAssetCategoryName\"\r\n                            :is-check-validate=\"this.isCheckValidate\" ref=\"FixedAssetCategoryName\">\r\n\r\n                        </minput>\r\n                        <div class=\"input__container \">\r\n\r\n                            <div class=\"input__content--flex\">\r\n                                <minput-number input-content=\"Nguyên giá\" :is-input-number=\"false\" :is-not-allow-null=\"true\"\r\n                                    :is-padding-right=\"true\" v-model=\"pelfInput.Cost\"\r\n                                    :is-check-validate=\"this.isCheckValidate\" ref=\"Cost\" :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                                </minput-number>\r\n\r\n                                <minput-number input-content=\"Số năm sử dụng \" :is-input-number=\"false\"\r\n                                    :is-not-allow-null=\"true\" :is-padding-right=\"true\" v-model=\"pelfInput.LifeTime\"\r\n                                    :is-check-validate=\"this.isCheckValidate\" ref=\"LifeTime\" :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                                </minput-number>\r\n\r\n\r\n\r\n                            </div>\r\n\r\n                        </div>\r\n                        <div class=\"input__container \">\r\n\r\n                            <div class=\"input__content--flex\">\r\n                                <minput-number input-content=\" Giá trị hao mòn năm \" :is-input-number=\"false\"\r\n                                    :is-not-allow-null=\"true\" :is-padding-right=\"true\" v-model=\"this.atrophyYear\"\r\n                                    :is-check-validate=\"this.isCheckValidate\" ref=\"atrophyYear\" :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                                </minput-number>\r\n                                <!-- <minput-number input-content=\" Năm theo dõi  \" :is-input-number=\"false\"\r\n                                    :is-not-allow-null=\"true\" :is-disable-and-padding-right=\"true\"\r\n                                    v-model=\"pelfInput.TrackedYear\" :is-check-validate=\"this.isCheckValidate\"\r\n                                    ref=\"TrackedYear\">\r\n\r\n                                </minput-number> -->\r\n                                <minput input-content=\"Năm theo dõi \" :is-input-number=\"false\" :is-not-allow-null=\"true\"\r\n                                    :is-disable-padding-right=\"true\" v-model=\"pelfInput.TrackedYear\"\r\n                                    :is-check-validate=\"this.isCheckValidate\" ref=\"TrackedYear\" :setMaxLength=\"this.$_MMaxLengthInput.number\">\r\n\r\n                                </minput>\r\n\r\n\r\n                            </div>\r\n\r\n                        </div>\r\n                        <div class=\"input__container \">\r\n\r\n                            <div class=\"input__content--flex\">\r\n                                <m-datapicker input-content=\"Ngày bắt đầu sử dụng \" :is-not-allow-null=\"true\"\r\n                                    v-model=\"pelfInput.DayUse\" :is-check-validate=\"this.isCheckValidate\"\r\n                                    ref=\"DayUse\"></m-datapicker>\r\n\r\n\r\n                                <div class=\"input__container\"></div>\r\n\r\n                            </div>\r\n\r\n                        </div>\r\n                    </div>\r\n\r\n                </div>\r\n\r\n\r\n            </div>\r\n\r\n            <div class=\"popup__pooter\">\r\n                <button class=\"button button--white\" @click=\"btnCloseUnSave\"> Hủy</button>\r\n                <button class=\"button button--blue\" @click=\"btnSave\">Lưu</button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <m-alert v-if=\"isShowAlert\" :status-alert=\"this.statusAlert\" :list-delete=\"[]\" @close=\"closeAlert\"\r\n        :alert-content-input=\"this.alertContent\"></m-alert>\r\n        <m-notification :informStatus=\"this.informStatus\" v-if=\"this.isShowInform\"\r\n        :informContent=\"this.informContent\"></m-notification>\r\n</template>\r\n<script>\r\n\r\nimport format from '@/js/format/format';\r\nimport MAlert from '@/components/base/MAlert.vue';\r\nimport pelfApi from '@/config/pelf/pelfapi';\r\nimport Minput from \"@/components/base/Minput.vue\";\r\nimport MinputNumber from \"@/components/base/MInputNumber.vue\";\r\nimport MDropdown from \"@/components/base/MDropdown.vue\";\r\nimport MDatapicker from \"@/components/base/MDatapicker.vue\";\r\nimport MNotification from '@/components/base/MNotification.vue';\r\nimport resource from \"@/js/resource/resource\";\r\nimport maxlengthinput from \"@/js/enum/maxlengthinput.js\";\r\nexport default {\r\n    components: { Minput, MDropdown, MDatapicker, MinputNumber, MAlert ,MNotification},\r\n    props: {\r\n        pelf: null,\r\n        formMode: null,\r\n\r\n\r\n    },\r\n    created() {\r\n        var now= new Date();\r\n        this.pelftDefault = JSON.stringify(this.pelf);\r\n        this.pelftDefault = JSON.parse(this.pelftDefault);\r\n        this.pelfInput = JSON.stringify(this.pelf);\r\n        this.pelfInput = JSON.parse(this.pelfInput);\r\n        if (this.formMode == this.$_MFormMode.FixAsset) {\r\n            this.popupTitle = resource.FormModeTitle.FixAsset\r\n            \r\n        }\r\n        else if (this.formMode == this.$_MFormMode.AddAsset) {\r\n            this.popupTitle = resource.FormModeTitle.AddAsset\r\n            this.pelfInput.TrackedYear =  now.getFullYear();\r\n        }\r\n        console.log(this.pelf);\r\n        console.log(this.formMode);\r\n        this.atrophyYear = this.pelfInput.Cost * this.pelfInput.DepreciationRate / 100;\r\n\r\n\r\n\r\n    },\r\n    methods: {\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Đóng mở cảnh báo \r\n         */\r\n        closeAlert(isForm) {\r\n            if (isForm) {\r\n                this.isShowAlert = false;\r\n                return;\r\n            }\r\n            if (this.inValue) {\r\n                this.isShowAlert = false;\r\n            }\r\n            else {\r\n                this.isShowAlert = false;\r\n                this.btnClose();\r\n            }\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Add list invalid vào 1 mảng \r\n         */\r\n        addListInValues(data, isPush) {\r\n            if (isPush) {\r\n                this.listInValue.push(data);\r\n            }\r\n            else {\r\n                this.listInValue = this.listInValue.filter(item => item !== data)\r\n            }\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Check null\r\n         */\r\n        checkNull(data) {\r\n            data=format.whiteSpace(data)\r\n        \r\n        if (!data) {\r\n            \r\n            return true;\r\n        }\r\n        else {\r\n            return false;\r\n        }\r\n        },\r\n        /**\r\n         * đóng form chi tiết tài sản \r\n         * Author: PXTRUONG, Create: 24/3/2023\r\n         */\r\n        btnClose() {\r\n            this.$emit(\"hiddePolfDetail\");\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(15/6/2023)\r\n         * Thực hiện gọi api thêm tài sản \r\n         */\r\n        async add() {\r\n            let option = {\r\n                method: 'POST',\r\n                headers: {\r\n                    'Content-Type':\r\n                        'application/json;charset=utf-8'\r\n                },\r\n                body: JSON.stringify(this.pelfInput)\r\n            }\r\n            console.log(JSON.stringify(this.pelfInput));\r\n            await fetch(pelfApi.fixedAsset.api, option)\r\n                .then(res => res.json())\r\n                .then(data => {\r\n                    if (data.IsSuccess) {\r\n                        this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Add);\r\n                    }\r\n                else{\r\n                        this.showInform(true, this.$_MInformStatus.IsFailed,data.UserMessage)\r\n                    }\r\n                })\r\n        },\r\n        /**\r\n        * Author:PXTRUONG(15/6/2023)\r\n        * Thực hiện gọi api sửa tài sản \r\n        */\r\n        async fix() {\r\n           \r\n            let optionFix = {\r\n                method: 'PUT',\r\n                headers: {\r\n                    'Content-Type':\r\n                        'application/json;charset=utf-8'\r\n                },\r\n                body: JSON.stringify(this.pelfInput)\r\n            }\r\n            console.log(JSON.stringify(this.pelfInput));\r\n            await fetch(pelfApi.fixedAsset.api, optionFix)\r\n                .then(res => res.json())\r\n                .then(data => {\r\n                    if (data.IsSuccess) {\r\n                        this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsSucess, this.$_MResource.InformContent.Fixed);\r\n                        \r\n                    }\r\n                    else {\r\n                        this.$emit(\"hiddePolfDetail\", true, this.$_MInformStatus.IsFailed, this.$_MResource.InformContent.Fixed);\r\n                    }\r\n                })\r\n\r\n        },\r\n         /**\r\n         * Author:PXTRUONG(19/6/2023)\r\n         * Thực hiện đóng mở thông báo chuyền nội dung thông báo \r\n         */\r\n         showInform(isShow, informStatus,content ) {\r\n            this.isShowInform = isShow;\r\n            this.informStatus = informStatus;\r\n            this.informContent = content;\r\n            setTimeout(() => this.isShowInform = false, 4000);\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(31/5/2023)\r\n         * Thực hiện lưu và đóng form \r\n         */\r\n        async btnSave() {\r\n            // this.btnClose();\r\n            try {\r\n                this.isCheckValidate = true;\r\n                for (const inputRef in this.$refs) {\r\n                    //tìm các input invalid\r\n                    if (this.checkNull(this.$refs[inputRef].value)) {\r\n                        console.log(this.$refs[inputRef].inputContent);\r\n                        this.inValue = true;\r\n                        this.isShowAlert = true;\r\n                        this.alertContent = this.$refs[inputRef].inputContent + \" không được bỏ trống\";\r\n\r\n                        return;\r\n                    }\r\n                }\r\n\r\n                // console.log(this.$refs[\"PurchaseDate\"]);\r\n                if (this.formMode == this.$_MFormMode.FixAsset) {\r\n                    this.fix();\r\n                }\r\n                else if (this.formMode == this.$_MFormMode.AddAsset) {\r\n                    this.add();\