{\r\n                    this.arraypagenumber.push(i);\r\n                }\r\n                else if (i == this.PageNumberInput + 3 || i == this.PageNumberInput - 3) {\r\n                    this.arraypagenumber.push(\"...\")\r\n                }\r\n\r\n            }\r\n\r\n\r\n        },\r\n        /**\r\n         * Author:PXTRUONG(20/6/2023)\r\n         * Thực hiện phân trang \r\n         */\r\n        async paging(pageNumber, keyWord, departmentName, categoryName) {\r\n            console.log(\"trang số: \", pageNumber);\r\n            console.log(\"số bản ghi\", this.PageSizeInput);\r\n             await fetch(pelfApi.fixedAsset.getPaging + '?PageSize=' + this.PageSizeInput +\r\n                '?PageNumber=' + pageNumber + \"?KeyWord=\" + keyWord + \"?DepartmentName=\" + departmentName + \"?FixedCategoryName\" + categoryName)\r\n                .then((res) => res.json())\r\n                .then((data) => {\r\n                    console.log(data);\r\n                });\r\n            \r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            listPage: [\r\n                {\r\n                    \"PageNumber\": \"20\"\r\n                },\r\n                {\r\n                    \"PageNumber\": \"30\"\r\n                },\r\n                {\r\n                    \"PageNumber\": \"40\"\r\n                },\r\n                {\r\n                    \"PageNumber\": \"50\"\r\n                }\r\n            ],\r\n            PageSizeInput: {\r\n                type: Number,\r\n                default: 20\r\n            },\r\n            PageNumberInput: null,\r\n            TotalPageNumber: null,\r\n            value: null,\r\n            arraypagenumber: []\r\n\r\n        }\r\n    },\r\n}\r\n</script>"],"mappings":";;EACSA,KAAK,EAAC;AAA0B;;EAC5BA,KAAK,EAAC;AAAqB;;EAErBA,KAAK,EAAC;AAA8B;;EACnBA,KAAK,EAAC;AAA2B;;EAIpDA,KAAK,EAAC;AAAgC;;EAItCA,KAAK,EAAC;AAAkB;gCACzBC,mBAAA,CAEM;EAFDD,KAAK,EAAC;AAAmC;;EAGzCA,KAAK,EAAC;AAAwB;;iCAUnCC,mB